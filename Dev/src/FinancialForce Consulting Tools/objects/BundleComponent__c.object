    <validationRules>
        <fullName>Blank_Apex_Code</fullName>
        <active>true</active>
        <description>If BundleComponent Type is Apex then ApexCode TextArea should not be blank.</description>
        <errorConditionFormula>CONTAINS( TEXT( Type__c ) , &apos;Apex&apos;)  &amp;&amp;  ISBLANK( ApexCode__c )</errorConditionFormula>
        <errorMessage>Enter Apex Code to be executed or choose BundleComponent Type other than &apos;Apex&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Blank_Package_Name</fullName>
        <active>true</active>
        <description>If Bundle Component Type is selected as Installed Package then Package Name should not be empty</description>
        <errorConditionFormula>CONTAINS( TEXT( Type__c ) , &apos;InstalledPackage&apos;)  &amp;&amp;  ISBLANK( PackageName__c )</errorConditionFormula>
        <errorMessage>Enter Package Name  to be installed or choose BundleComponent Type other than &apos;InstalledPackage&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Blank_Package_Version</fullName>
        <active>true</active>
        <description>If Bundle Component Type is selected as Installed Package then Package  Version should not be empty</description>
        <errorConditionFormula>CONTAINS( TEXT( Type__c ) , &apos;InstalledPackage&apos;)  &amp;&amp;  ISBLANK(  PackageVersion__c )</errorConditionFormula>
        <errorMessage>Enter Package Version to be installed or choose BundleComponent Type other than &apos;InstalledPackage&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BundleComponent_Type_Mandatory</fullName>
        <active>true</active>
        <description>Type field of BundleComponent Object is mandatory</description>
        <errorConditionFormula>ISBLANK( TEXT( Type__c ) )</errorConditionFormula>
        <errorMessage>BundleComponent Type is mandatory field. It cannot be left blank.</errorMessage>
    </validationRules>
