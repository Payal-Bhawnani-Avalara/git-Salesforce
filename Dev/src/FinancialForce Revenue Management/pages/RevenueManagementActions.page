<apex:page showHeader="true" sidebar="false" tabStyle="RevenueManagementActions__tab">
	<style>
		.actionsContent h3 {
			font-size: 1.5em;
			font-weight: bold;
			margin: 20px 10px;
			display: block;
		}

		#actionsNav ul {
			list-style-type: disc;
			margin: 0 20px;
			padding: 0 0 20px 30px;
		}

		#actionsNav ul li {
			list-style-type: disc;
		}
	</style>
	<c:VFShim />
	<apex:variable var="isLightening" value="{!$User.UIThemeDisplayed == 'Theme4d'}"/>
	<apex:sectionHeader title="Revenue Management" subtitle="Actions"/>
	<div class="actionsContent">
		<apex:pageBlock >
			<h3>Actions</h3>
			<nav id="actionsNav">
				<ul>
					<li><apex:outputLink value="{!$Page.ffrr__TransferPreviouslyRecognized}">{!$Label.ffrr__transferpreviouslyrecognized}</apex:outputLink></li>
					<li><apex:outputLink value="{!$Page.ffrr__DeleteDraftRecords}">{!$Label.ffrr__deletedraftforecasts}</apex:outputLink></li>
					<li><apex:outputLink value="{!$Page.ffrr__ActualVForecastManage}">{!$Label.ffrr__manageavfs}</apex:outputLink></li>
					<li><apex:outputLink value="{!$Page.ffrr__Administration}">{!$Label.ffrr__administrationoperationdeletestagingdata}</apex:outputLink></li>
				</ul>
			</nav>
		</apex:pageBlock>
	</div>
</apex:page>