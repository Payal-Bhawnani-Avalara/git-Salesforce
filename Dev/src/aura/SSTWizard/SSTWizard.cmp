             </div>
             
             <!-- buttons  -->
             <div class="slds-grid  slds-grid--align-center">
                <aura:renderIf isTrue="{!v.showIntro}">
                    <button class="slds-button slds-button--brand" onclick="{!c.nextTab}">Get Started</button>
                </aura:renderIf>
                
                <aura:renderIf isTrue="{!v.showComInfo}">
                    <button class="slds-button slds-button--brand" onclick="{!c.prevTab}" >Back</button>
                    <button class="slds-button slds-button--brand" onclick="{!c.nextTab}" >Next</button>
                </aura:renderIf>
                
                <aura:renderIf isTrue="{!v.showSSTState}">        
                    <button class="slds-button slds-button--brand" onclick="{!c.prevTab}" >Back</button> 
                    <button class="slds-button slds-button--brand" onclick="{!c.nextTab}" >Next</button>
                </aura:renderIf>
                  <aura:renderIf isTrue="{!v.showNexus}">        
                 <button class="slds-button slds-button--brand" onclick="{!c.prevTab}" >Back</button> 
                    <button class="slds-button slds-button--brand" onclick="{!c.nextTab}" >Next</button>
                </aura:renderIf>
                <aura:renderIf isTrue="{!v.showData}">        
                    <button class="slds-button slds-button--brand" onclick="{!c.prevTab}" >Back</button> 
                    <button class="slds-button slds-button--brand" onclick="{!c.saveRecord}" >Save</button>
                </aura:renderIf>
                
            </div>
    </div>
    </div>
</aura:component>