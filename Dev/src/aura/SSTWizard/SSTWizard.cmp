<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,lightning:isUrlAddressable" access="global"   controller="SSTWizardController" >
    <aura:attribute name="ObjectName" type="String" default="SST_Onboarding__c" access="global"/> <!-- Object Name as String-->
    <aura:attribute name="sstData" type="SST_Onboarding__c" default="{ 'sobjectType': 'SST_Onboarding__c','Business_Entity_Type__c': ''}"/>
    <aura:attribute name="showIntro" type="boolean" default="true"/> 
    <aura:attribute name="showComInfo" type="boolean" default="false"/> 
    <aura:attribute name="showSSTState" type="boolean" default="false"/>
    <aura:attribute name="showNexus" type="boolean" default="false"/>
    <aura:attribute name="showReg" type="boolean" default="false"/>
     
    <aura:attribute name="businessEntityType" type="String"  default="Business_Entity_Type__c" access="global"/>
    <aura:attribute name="businessEntityTypePicklist" type="String[]"/>
    <aura:attribute name="sstState" type="String"  default="SST_State_Selection__c" access="global"/>
    <aura:attribute name="sstStatePicklist" type="List" default="[]"/>
    <aura:attribute name="sstStatePicklistselected" type="List" default="[]"/>
    
    <aura:attribute name="stateOfIn" type="String"  default="State_of_Incorporation_or_Organization__c" access="global"/>
    <aura:attribute name="stateOfInPicklist" type="List" default="[]"/>
    
    <!-- AR Nexus -->
    <aura:attribute name="arRegistered" type="String"  default="Arkansas_Registered__c" access="global"/>
    <aura:attribute name="arRegisteredPicklist" type="List" default="[]"/>
    <aura:attribute name="arFil" type="String"  default="Arkansas_Filing_Frequency__c" access="global"/>
    <aura:attribute name="arFilPicklist" type="List" default="[]"/>
    
     <!-- AR SST Registration Information -->
    <aura:attribute name="arSSTV" type="String"  default="ArkansasSSTVolunteer__c" access="global"/>
    <aura:attribute name="arSSTVPicklist" type="List" default="[]"/>
    <aura:attribute name="arNV" type="String"  default="Arkansas_Non_Volunteer__c" access="global"/>
    <aura:attribute name="arNVPicklist" type="List" default="[]"/>
    
    <!-- Georgia Nexus -->
    <aura:attribute name="geRegistered" type="String"  default="Georgia_Registered__c" access="global"/>
    <aura:attribute name="geRegisteredPicklist" type="List" default="[]"/>
    <aura:attribute name="geFil" type="String"  default="Georgia_Filing_Frequency__c" access="global"/>
    <aura:attribute name="geFilPicklist" type="List" default="[]"/>
    
    <!-- Indiana Returns -->
    <aura:attribute name="inRetunrs" type="String"  default="IN_returns__c" access="global"/>
    <aura:attribute name="inRetunrsPicklist" type="List" default="[]"/>
    
    <aura:attribute name="FEIN" type="String" />
    <aura:attribute name="showCo" type="boolean" default="false" access="global"/>
    <aura:attribute name="showSP" type="boolean" default="false" access="global"/>
    
    <!-- SST States Display  -->
    <aura:attribute name="showAr" type="boolean" default="false" access="global"/>
    <aura:attribute name="showGe" type="boolean" default="false" access="global"/>
    <aura:attribute name="showIn" type="boolean" default="false" access="global"/>
    <aura:attribute name="showIo" type="boolean" default="false" access="global"/>
    <aura:attribute name="showKa" type="boolean" default="false" access="global"/>
    <aura:attribute name="showKe" type="boolean" default="false" access="global"/>
    <aura:attribute name="showMi" type="boolean" default="false" access="global"/>
    <aura:attribute name="showMn" type="boolean" default="false" access="global"/>
    <aura:attribute name="showNe" type="boolean" default="false" access="global"/>
    <aura:attribute name="showNv" type="boolean" default="false" access="global"/>
    <aura:attribute name="showNj" type="boolean" default="false" access="global"/> 
    <aura:attribute name="showNc" type="boolean" default="false" access="global"/>
    <aura:attribute name="showNd" type="boolean" default="false" access="global"/>
    <aura:attribute name="showOh" type="boolean" default="false" access="global"/>
    <aura:attribute name="showOk" type="boolean" default="false" access="global"/>
    <aura:attribute name="showRi" type="boolean" default="false" access="global"/>
    <aura:attribute name="showSd" type="boolean" default="false" access="global"/>
    <aura:attribute name="showTe" type="boolean" default="false" access="global"/>
    <aura:attribute name="showUt" type="boolean" default="false" access="global"/> 
    <aura:attribute name="showVe" type="boolean" default="false" access="global"/> 
    <aura:attribute name="showWa" type="boolean" default="false" access="global"/> 
    <aura:attribute name="showWv" type="boolean" default="false" access="global"/>
    <aura:attribute name="showWi" type="boolean" default="false" access="global"/>
    <aura:attribute name="showWy" type="boolean" default="false" access="global"/> 
    
    <!-- Volunteer False -->
    <aura:attribute name="arFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="geFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="inFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="ioFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="kaFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="keFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="miFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="mnFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="neFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="nvFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="njFalse" type="boolean" default="false" access="global"/> 
    <aura:attribute name="ncFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="ndFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="ohFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="okFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="riFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="sdFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="tefalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="utFalse" type="boolean" default="false" access="global"/> 
    <aura:attribute name="veFalse" type="boolean" default="false" access="global"/> 
    <aura:attribute name="waFalse" type="boolean" default="false" access="global"/> 
    <aura:attribute name="wvFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="wiFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="wyFalse" type="boolean" default="false" access="global"/> 
     
    <aura:attribute name="setMessage" type="String"/> 
    <aura:attribute name="showError" type="boolean" default="false"/>
    <aura:attribute name="showData" type="boolean" default="false"/>
    <aura:attribute name="message" type="String" default="NoError"/> 
   
    <div class="slds-grid slds-grid--vertical forceListViewManager adapetr-config-main">
       <!--********* HEADER ********* -->
       <div class="slds-page-header adapetr-header">
           <div class="slds-media__body">
               <c:SSTWizardHeader></c:SSTWizardHeader>
               <div id="showErrorrTractConfig" class="cmn_error_msg " style="display:none; text-align:center;font-weight:700">
                   <ui:outputText value="{!v.message}"/>
               </div>
               <div id="showMessageTractConfig" class="cmn_message " style="display:none;text-align:center;font-weight:700">
                   <ui:outputText value="{!v.message}"/>
               </div>
           </div>
       </div>
        
       <div class="adapetr-config-sec">
           <div class="slds-box slds-align_absolute-center config-table-details">
               <table class="slds-table ">
                 <aura:renderIf isTrue="{!v.showError}">  
                       <ui:message title="Warning" severity="warning" closable="true">
                           Please Fill required field first </ui:message>
                 </aura:renderIf>
                 
                 <!-- Inroduction Page -->
                 <aura:renderIf isTrue="{!v.showIntro}">
                       <c:SSTText/>  
                 </aura:renderIf>
                  
                 <!-- Company Information -->
                 <aura:renderIf isTrue="{!v.showComInfo}">
                 <div class="slds-align_absolute-center" >
                   <fieldset class="slds-box slds-theme--default slds-container_medium">
                   <legend id="newdemform" class="slds-text-heading--small 
	                  slds-p-vertical--medium">
	                Company Information
	               </legend>
                   <div class="slds-grid slds-wrap" >
                      <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large" >
                          <lightning:select aura:id="fieldId" name="entityValue" label="Business Entity Type"  required="true" onchange="{!c.beType}">
                              <aura:iteration var="option" items="{! v.businessEntityTypePicklist }">
                                  <option value="{! option }" >{! option }</option>
                              </aura:iteration>
                          </lightning:select>                      
                      </span> 
                            
                     
                     <aura:if isTrue="{!v.showCo}">
                      <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
                      <lightning:input aura:id="newSSTform" label="FEIN" name="FEIN" value="{!v.sstData.FEINM__c}"  required="true" />
                      </span>
                      <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
                      <lightning:input aura:id="newSSTform" label="Legal Business Name" name="Legal Business Name" value="{!v.sstData.Legal_Business_Name__c}"  required="true"/>
                      </span>
                      <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
                      <lightning:input aura:id="newSSTform" label="DBA Business Name" name="DBA Business Name" value="{!v.sstData.DBA_Business_Name__c}"  required="true" />
                      </span>
                     </aura:if>
                     
                      <aura:if isTrue="{!v.showSP}">
                      <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
                      <lightning:input aura:id="newSSTform" label="SSN" name="SSN" value="{!v.sstData.SSN__c}"  required="true" />
                      </span>
                      <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
                      <lightning:input aura:id="newSSTform" label="First Name" name="First Name" value="{!v.sstData.First_Name__c}"  required="true"/>
                      </span>
                      <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
                      <lightning:input aura:id="newSSTform" label="Middle Name" name="Middle Name" value="{!v.sstData.Middle_Name__c}"  required="true"/>
                      </span>
                      <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
                      <lightning:input aura:id="newSSTform" label="Last Name" name="Last Name" value="{!v.sstData.First_Name__c}"  required="true" />
                      </span>
                     </aura:if>
                       
                       <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large" >
                            <lightning:select aura:id="stateId" name="stateOfIn" label="State of Incorporation or Organization"  required="true" >
                                <aura:iteration var="option" items="{! v.stateOfInPicklist }">
                                    <option value="{! option }" >{! option }</option>
                                </aura:iteration>
                            </lightning:select>                      
                        </span> 
                        
                        <div aria-labelledby="newcompanyform" class="slds-align_absolute-center">
			            <!-- BOXED AREA -->
			            <fieldset class="slds-box slds-theme--default slds-container_medium">
			               <legend id="newdemform" class="slds-text-heading--small 
			                  slds-p-vertical--medium">
			                 Company Corporate Address
			               </legend> 
                        <div class="slds-grid slds-wrap">
                        <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
                        <lightning:input aura:id="ComCo" label="Address Line 1" name="Address Line 1" value="{!v.sstData.Company_Corporate_Address_Address_Line__c}"  required="true" />
                        </span>
                        <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
                        <lightning:input aura:id="ComCo" label="Address Line 2" name="First Name" value="{!v.sstData.CompanyCorporate_Address_Address_Line2__c}"  required="true"/>
                        </span>
                        <span class="slds-size_1-of-2 slds-max-small-size_1-of-2 slds-p-horizontal_medium slds-p-bottom_large"> 
                        <lightning:input aura:id="ComCo" label="City" name="City" value="{!v.sstData.Company_Corporate_Address_City__c}"  required="true"/>
                        </span>
                        <span class="slds-size_1-of-2 slds-max-small-size_1-of-2 slds-p-horizontal_medium slds-p-bottom_large"> 
                        <lightning:input aura:id="ComCo" label="State" name="State" value="{!v.sstData.Company_Corporate_Address_State__c}"  required="true" />
                        </span>
                        <span class="slds-size_1-of-2 slds-max-small-size_1-of-2 slds-p-horizontal_medium slds-p-bottom_large"> 
                        <lightning:input aura:id="ComCo" label="Zip" name="Zip" value="{!v.sstData.Company_Corporate_Address_Zip_Code__c}"  required="true" />
                        </span>
                         <span class="slds-size_1-of-2 slds-max-small-size_1-of-2 slds-p-horizontal_medium slds-p-bottom_large"> 
                        <lightning:input aura:id="ComCo" label="Country" name="Country" value="{!v.sstData.Company_Corporate_Address_Country__c}"  required="true" />
                        </span>
                         <span class="slds-size_1-of-2 slds-max-small-size_1-of-2 slds-p-horizontal_medium slds-p-bottom_large"> 
                        <lightning:input aura:id="ComCo" label="Phone Number" name="Phone Number" value="{!v.sstData.Company_Corporate_Phone_Number__c}"  required="true" />
                        </span>
                         <span class="slds-size_1-of-2 slds-max-small-size_1-of-2 slds-p-horizontal_medium slds-p-bottom_large"> 
                        <lightning:input aura:id="ComCo" label="Phone Extension" name="Phone Extension" value="{!v.sstData.Company_Corporate_Phone_Extension__c}"  required="true" />
                        </span>
                       </div>
                       </fieldset>
                      </div>
                      
                       <!--Company Contact Information-->
                       <div class="slds-align_absolute-center" >
                      <fieldset class="slds-box slds-theme--default slds-container_medium">
                      <legend id="newdemform" class="slds-text-heading--small 
				                  slds-p-vertical--medium">
				               Company Contact Information
				               </legend>
                    	 <div class="slds-grid slds-wrap" >                        
                         <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
                         <lightning:input aura:id="newSSTform" label="First Name" name="First Name" value="{!v.sstData.First_Name__c}"  required="true"/>
                         </span>
                         <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
                         <lightning:input aura:id="newSSTform" label="Middle Name" name="Middle Name" value="{!v.sstData.Middle_Name__c}"  required="true"/>
                         </span>
                         <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
                         <lightning:input aura:id="newSSTform" label="Last Name" name="Last Name" value="{!v.sstData.First_Name__c}"  required="true" />
                         </span>
                          <span class="slds-size_1-of-2 slds-max-small-size_1-of-2 slds-p-horizontal_medium slds-p-bottom_large"> 
                         <lightning:input aura:id="ComCo" label="Phone Number" name="Phone Number" value="{!v.sstData.Company_Corporate_Phone_Number__c}"  required="true" />
                         </span>
                          <span class="slds-size_1-of-2 slds-max-small-size_1-of-2 slds-p-horizontal_medium slds-p-bottom_large"> 
                         <lightning:input aura:id="ComCo" label="Phone Extension" name="Phone Extension" value="{!v.sstData.Company_Corporate_Phone_Extension__c}"  required="true" />
                         </span>   
                                       
                      </div>
                     </fieldset>
                    </div>
                   </div>
                  </fieldset>
                </div>
              </aura:renderIf>   
              
              <!-- SST State -->
              <aura:renderIf isTrue="{!v.showSSTState}">
                 <div class="slds-align_absolute-center" >
                     <fieldset class="slds-box slds-theme--default slds-container_medium">
                     <legend id="newdemform" class="slds-text-heading--small 
			                  slds-p-vertical--medium">
			               SST State Selection
			               </legend>
                <div class="slds-grid slds-wrap" >
                <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large">
                    <lightning:checkboxGroup aura:id="sstStateId" name="sstStatePicklistselected"
                       label="Select the states in which you want to register as part of the SST program:"
                       options="{! v.sstStatePicklist }" value = "{! v.sstStatePicklistselected }" onchange= "{!c.sstNexus}"
                       required="true"/>
                </span>
                </div>
               </fieldset>
              </div>
             </aura:renderIf>  
             
              <!-- Nexus Information -->
              <aura:renderIf isTrue="{!v.showNexus}">
                 <div class="slds-align_absolute-center" >
                     <fieldset class="slds-box slds-theme--default slds-container_medium">
                     <legend id="newdemform" class="slds-text-heading--small 
			                  slds-p-vertical--medium">
			               Nexus Information
			               </legend>
                <div class="slds-grid slds-wrap" >
                 <div class="slds-col slds-size_1-of-1 ">Please select the current registration status and filing frequencies for the selected SST states.</div>
                 <br/>
                 <br/>
                 <aura:if isTrue="{!v.showAr}">
                <span class="slds-size_1-of-2 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large" style="width:300px;">
                     <lightning:select aura:id="fieldId" name="arRegistered" label="Arkansas - Registered"  required="true">
                              <aura:iteration var="option" items="{! v.arRegisteredPicklist }">
                                  <option value="{! option }" >{! option }</option>
                              </aura:iteration>
                          </lightning:select>    
                </span>
                <span class="slds-size_1-of-2 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large" style="width:300px;">
                     <lightning:select aura:id="fieldId" name="entityValue" label="Arkansas Filing Frequency"  required="true">
                              <aura:iteration var="option" items="{! v.arFilPicklist }">
                                  <option value="{! option }" >{! option }</option>
                              </aura:iteration>
                          </lightning:select>    
                </span>
                </aura:if>
                <aura:if isTrue="{!v.showGe}">
                <span class="slds-size_1-of-2 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large" style="width:300px;">
                     <lightning:select aura:id="fieldId" name="arRegistered" label="Georgia - Registered"  required="true">
                              <aura:iteration var="option" items="{! v.arRegisteredPicklist }">
                                  <option value="{! option }" >{! option }</option>
                              </aura:iteration>
                          </lightning:select>    
                </span>
                <span class="slds-size_1-of-2 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large" style="width:300px;">
                     <lightning:select aura:id="fieldId" name="entityValue" label="Georgia Filing Frequency"  required="true">
                              <aura:iteration var="option" items="{! v.arFilPicklist }">
                                  <option value="{! option }" >{! option }</option>
                              </aura:iteration>
                          </lightning:select>    
                </span>
                </aura:if>
                 <aura:if isTrue="{!v.showIn}">
                <span class="slds-size_1-of-2 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large" style="width:300px;">
                     <lightning:select aura:id="fieldId" name="arRegistered" label="Indiana - Registered"  required="true">
                              <aura:iteration var="option" items="{! v.arRegisteredPicklist }">
                                  <option value="{! option }" >{! option }</option>
                              </aura:iteration>
                          </lightning:select>    
                </span>
                <span class="slds-size_1-of-2 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large" style="width:300px;">
                     <lightning:select aura:id="fieldId" name="entityValue" label="Indiana Filing Frequency"  required="true">
                              <aura:iteration var="option" items="{! v.arFilPicklist }">
                                  <option value="{! option }" >{! option }</option>
                              </aura:iteration>
                          </lightning:select>    
                </span>
                 <span class="slds-size_1-of-2 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large" style="width:500px;">
                     <lightning:select aura:id="fieldId" name="entityValue" label="Please identify the collection allowance percentage that the state has notified you to include on your IN returns?"  required="true">
                              <aura:iteration var="option" items="{! v.inRetunrsPicklist}">
                                  <option value="{! option }" >{! option }</option>
                              </aura:iteration>
                          </lightning:select>    
                </span>
                
                </aura:if>
                 <aura:if isTrue="{!v.showIo}">
                <span class="slds-size_1-of-2 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large" style="width:300px;">
                     <lightning:select aura:id="fieldId" name="arRegistered" label="Iowa - Registered"  required="true">
                              <aura:iteration var="option" items="{! v.arRegisteredPicklist }">
                                  <option value="{! option }" >{! option }</option>
                              </aura:iteration>
                          </lightning:select>    
                </span>
                <span class="slds-size_1-of-2 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large" style="width:300px;">
                     <lightning:select aura:id="fieldId" name="entityValue" label="Iowa Filing Frequency"  required="true">
                              <aura:iteration var="option" items="{! v.arFilPicklist }">
                                  <option value="{! option }" >{! option }</option>
                              </aura:iteration>
                          </lightning:select>    
                </span>
                </aura:if>
                <aura:if isTrue="{!v.showKa}">
                <span class="slds-size_1-of-2 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large" style="width:300px;">
                     <lightning:select aura:id="fieldId" name="arRegistered" label="Kansas - Registered"  required="true">
                              <aura:iteration var="option" items="{! v.arRegisteredPicklist }">
                                  <option value="{! option }" >{! option }</option>
                              </aura:iteration>
                          </lightning:select>    
                </span>
                <span class="slds-size_1-of-2 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large" style="width:300px;">
                     <lightning:select aura:id="fieldId" name="entityValue" label="Kansas Filing Frequency"  required="true">
                              <aura:iteration var="option" items="{! v.arFilPicklist }">
                                  <option value="{! option }" >{! option }</option>
                              </aura:iteration>
                          </lightning:select>    
                </span>
