<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,lightning:isUrlAddressable" access="global"   controller="SSTWizardController" >
    <aura:attribute name="ObjectName" type="String" default="SST_Onboarding__c" access="global"/> <!-- Object Name as String-->
    <aura:attribute name="sstData" type="SST_Onboarding__c" default="{ 'sobjectType': 'SST_Onboarding__c','Business_Entity_Type__c': ''}"/>
    <aura:attribute name="showIntro" type="boolean" default="true"/> 
    <aura:attribute name="showComInfo" type="boolean" default="false"/> 
    <aura:attribute name="showRandom" type="boolean" default="false"/>
    <aura:attribute name="showSSTState" type="boolean" default="false"/>
    <aura:attribute name="showNexus" type="boolean" default="false"/>
    <aura:attribute name="showRemoteState" type="boolean" default="false"/>
    <aura:attribute name="showReg" type="boolean" default="false"/>
    <aura:attribute name="showRegSSN" type="boolean" default="false"/>
    <aura:attribute name="showAfInfo" type="boolean" default="false"/>
    <aura:attribute name="showStats" type="boolean" default="false"/>
    <aura:attribute name="errors" type="List"/>
    
    <!-- Picklist  -->
    <aura:attribute name="businessEntityType" type="String"  default="Business_Entity_Type__c" access="global"/>
    <aura:attribute name="businessEntityTypePicklist" type="String[]"/>
    <aura:attribute name="sstState" type="String"  default="SST_State_Selection__c" access="global"/>
    <aura:attribute name="sstStatePicklist" type="List" default="[]"/>
    <aura:attribute name="sstStatePicklistselected" type="List" default="[]"/>
    <aura:attribute name="regSST" type="String"  default="Registered_with_the_SST_Governing_Board__c" access="global"/>
    <aura:attribute name="regSSTPicklist" type="String[]"/>
    <aura:attribute name="comAff" type="String"  default="Is_your_company_affiliated__c" access="global"/>
    <aura:attribute name="comAffPicklist" type="String[]"/>
    <aura:attribute name="typeBus" type="String"  default="Type_of_Business__c" access="global"/>
    <aura:attribute name="typeBusPicklist" type="String[]"/>
    <aura:attribute name="sstProg" type="String"  default="introduced_to_the_SST_program__c" access="global"/>
    <aura:attribute name="sstProgPicklist" type="String[]"/>
    <aura:attribute name="joinSSTProg" type="String"  default="Why_are_you_joining_the_SST_program__c" access="global"/>
    <aura:attribute name="joinSSTProgPicklist" type="String[]"/>
    <aura:attribute name="sstRemote" type="String"  default="Arkansas_Remote__c" access="global"/>
    <aura:attribute name="sstRemotePicklist" type="String[]"/>
    <aura:attribute name="waBOC" type="String"  default="Washington_monthly_WA_B_O__c" access="global"/>
    <aura:attribute name="waBOCPicklist" type="String[]"/>
    
    <aura:attribute name="stateOfIn" type="String"  default="State_of_Incorporation_or_Organization__c" access="global"/>
    <aura:attribute name="stateOfInPicklist" type="List" default="[]"/>
    
    <!-- AR Nexus -->
    <aura:attribute name="arRegistered" type="String"  default="Arkansas_Registered__c" access="global"/>
    <aura:attribute name="arRegisteredPicklist" type="List" default="[]"/>
    <aura:attribute name="arFil" type="String"  default="Arkansas_Filing_Frequency__c" access="global"/>
    <aura:attribute name="arFilPicklist" type="List" default="[]"/>
    
     <!-- AR SST Registration Information -->
    <aura:attribute name="arSSTV" type="String"  default="ArkansasSSTVolunteer__c" access="global"/>
    <aura:attribute name="arSSTVPicklist" type="List" default="[]"/>
    <aura:attribute name="arNV" type="String"  default="Arkansas_NV_Reason__c" access="global"/>
    <aura:attribute name="arNVPicklist" type="List" default="[]"/>
    
    <!-- Georgia Nexus -->
    <aura:attribute name="geRegistered" type="String"  default="Georgia_Registered__c" access="global"/>
    <aura:attribute name="geRegisteredPicklist" type="List" default="[]"/>
    <aura:attribute name="geFil" type="String"  default="Georgia_Filing_Frequency__c" access="global"/>
    <aura:attribute name="geFilPicklist" type="List" default="[]"/>
    
    <!-- Indiana Returns -->
    <aura:attribute name="inRetunrs" type="String"  default="IN_returns__c" access="global"/>
    <aura:attribute name="inRetunrsPicklist" type="List" default="[]"/>
    
    <aura:attribute name="FEIN" type="String" />
    <aura:attribute name="showCo" type="boolean" default="false" access="global"/>
    <aura:attribute name="showSP" type="boolean" default="false" access="global"/>
    <aura:attribute name="sameAsMailing" type="boolean" default="false" access="global"/>
    
    <!-- SST States Display  -->
    <aura:attribute name="showAr" type="boolean" default="false" access="global"/>
    <aura:attribute name="showGe" type="boolean" default="false" access="global"/>
    <aura:attribute name="showIn" type="boolean" default="false" access="global"/>
    <aura:attribute name="showIo" type="boolean" default="false" access="global"/>
    <aura:attribute name="showKa" type="boolean" default="false" access="global"/>
    <aura:attribute name="showKe" type="boolean" default="false" access="global"/>
    <aura:attribute name="showMi" type="boolean" default="false" access="global"/>
    <aura:attribute name="showMn" type="boolean" default="false" access="global"/>
    <aura:attribute name="showNe" type="boolean" default="false" access="global"/>
    <aura:attribute name="showNv" type="boolean" default="false" access="global"/>
    <aura:attribute name="showNj" type="boolean" default="false" access="global"/> 
    <aura:attribute name="showNc" type="boolean" default="false" access="global"/>
    <aura:attribute name="showNd" type="boolean" default="false" access="global"/>
    <aura:attribute name="showOh" type="boolean" default="false" access="global"/>
    <aura:attribute name="showOk" type="boolean" default="false" access="global"/>
    <aura:attribute name="showRi" type="boolean" default="false" access="global"/>
    <aura:attribute name="showSd" type="boolean" default="false" access="global"/>
    <aura:attribute name="showTe" type="boolean" default="false" access="global"/>
    <aura:attribute name="showUt" type="boolean" default="false" access="global"/> 
    <aura:attribute name="showVe" type="boolean" default="false" access="global"/> 
    <aura:attribute name="showWa" type="boolean" default="false" access="global"/> 
    <aura:attribute name="showWv" type="boolean" default="false" access="global"/>
    <aura:attribute name="showWi" type="boolean" default="false" access="global"/>
    <aura:attribute name="showWy" type="boolean" default="false" access="global"/> 
    
    <!-- Volunteer False -->
    <aura:attribute name="arFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="geFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="inFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="ioFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="kaFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="keFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="miFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="mnFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="neFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="nvFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="njFalse" type="boolean" default="false" access="global"/> 
    <aura:attribute name="ncFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="ndFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="ohFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="okFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="riFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="sdFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="tefalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="utFalse" type="boolean" default="false" access="global"/> 
    <aura:attribute name="veFalse" type="boolean" default="false" access="global"/> 
    <aura:attribute name="waFalse" type="boolean" default="false" access="global"/> 
    <aura:attribute name="wvFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="wiFalse" type="boolean" default="false" access="global"/>
    <aura:attribute name="wyFalse" type="boolean" default="false" access="global"/> 
    
    <!-- Statistical  Other -->
    <aura:attribute name="showSSTProgOther" type="boolean" default="false" access="global"/>
    <aura:attribute name="showSSTJoinOther" type="boolean" default="false" access="global"/>
     
    <aura:attribute name="setMessage" type="String"/> 
    <aura:attribute name="showError" type="boolean" default="false"/>
    <aura:attribute name="showData" type="boolean" default="false"/>
    <aura:attribute name="message" type="String" default="NoError"/> 
   
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    
    <div class="slds-grid slds-grid--vertical forceListViewManager adapetr-config-main">
       <!--********* HEADER ********* -->
       <div class="slds-page-header adapetr-header">
           <div class="slds-media__body">
               <c:SSTWizardHeader></c:SSTWizardHeader>
               <div id="showErrorrTractConfig" class="cmn_error_msg " style="display:none; text-align:center;font-weight:700">
                   <ui:outputText value="{!v.message}"/>
               </div>
               <div id="showMessageTractConfig" class="cmn_message " style="display:none;text-align:center;font-weight:700">
                   <ui:outputText value="{!v.message}"/>
               </div>
           </div>
       </div>
        
       <div class="adapetr-config-sec">
	       <div class="slds-align_absolute-center config-table-details">
	           <table class="slds-table ">
	             <aura:renderIf isTrue="{!v.showError}">  
	                   <ui:message title="Warning" severity="warning" closable="true">
	                       Please Fill required field first </ui:message>
	             </aura:renderIf>
	             
	             <!--********* Inroduction Page ********* -->
	             <aura:renderIf isTrue="{!v.showIntro}">
	                   <c:SSTText/>  
	             </aura:renderIf>
	              
	             <!--*********  Company Information ********* -->
	             <aura:renderIf isTrue="{!v.showComInfo}">
		             <div class="slds-align_absolute-center" >
		               <fieldset class="slds-box slds-theme--default slds-container_medium">
			               <legend id="newdemform" class="slds-text-heading--small 
			               slds-p-vertical--medium">  Company Information   </legend>		          
			               <div class="slds-grid slds-wrap" >
			                  <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large" >
			                      <lightning:select aura:id="entityTypeId" name="entityValue" label="Business Entity Type" required="true"  value="{!v.sstData.Business_Entity_Type__c}" onchange="{!c.beType}">
			                          <aura:iteration var="option" items="{! v.businessEntityTypePicklist }">
			                              <option value="{! option }" >{! option }</option>
			                          </aura:iteration>
			                      </lightning:select>                      
			                  </span> 
			                        			                 
			                 <aura:if isTrue="{!v.showCo}">
			                  <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
			                  <lightning:input aura:id="newSSTform" label="FEIN" name="FEIN" value="{!v.sstData.FEINM__c}" maxlength="11" pattern="[0-9]{2}-[0-9]{7}" 
			                  messageWhenPatternMismatch="Seller FEIN must be in this format: 99-9999999." required="true" />
			                  </span>
			                  <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
			                  <lightning:input aura:id="newSSTform" label="Legal Business Name" name="Legal Business Name" value="{!v.sstData.Legal_Business_Name__c}" 
			                  pattern="^[a-zA-Z0-9]*$" messageWhenPatternMismatch="Please provide valid input." maxlength="75"   required="true"/>
			                  </span>
			                  <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
			                  <lightning:input aura:id="newSSTform" label="DBA Business Name" name="DBA Business Name" maxlength="75"  value="{!v.sstData.DBA_Business_Name__c}"  
			                   pattern="^[a-zA-Z0-9]*$" messageWhenPatternMismatch="Please provide valid input." />
			                  </span>
			                 </aura:if>
			                 
			                  <aura:if isTrue="{!v.showSP}">
				                  <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
				                  <lightning:input aura:id="newSSTform" label="SSN" name="SSN" value="{!v.sstData.SSN__c}"  pattern="[0-9]{3}-[0-9]{2}-[0-9]{4}"  maxlength="11" 
				                   messageWhenPatternMismatch="SSN must be in this format: 999-99-9999."  required="true" />
				                   <aura:iteration items="{!v.errors}" var="error">{!errors}</aura:iteration>
				                  </span>
				                  <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
				                  <lightning:input aura:id="newSSTform" label="First Name" name="First Name" value="{!v.sstData.SP_First_Name__c}" maxlength="16"
				                   pattern="^[a-zA-Z0-9]*$" messageWhenPatternMismatch="Please provide valid input."    required="true"/>
				                  </span>
				                  <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
				                  <lightning:input aura:id="newSSTform" label="Middle Name" name="Middle Name" value="{!v.sstData.SP_Middle_Name__c}" maxlength="16" 
				                   pattern="^[a-zA-Z0-9]*$" messageWhenPatternMismatch="Please provide valid input."  required="true"/>
				                  </span>
				                  <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
				                  <lightning:input aura:id="newSSTform" label="Last Name" name="Last Name" value="{!v.sstData.SP_Last_Name__c}" maxlength="32" 
				                   pattern="^[a-zA-Z0-9]*$" messageWhenPatternMismatch="Please provide valid input."   required="true" />
				                  </span>
			                 </aura:if>
			                   
			                   <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large" >
		                        <lightning:select aura:id="stateId" name="stateOfIn" label="State of Incorporation or Organization" value="{!v.sstData.State_of_Incorporation_or_Organization__c}"  required="true" >
		                            <aura:iteration var="option" items="{! v.stateOfInPicklist }">
		                                <option value="{! option }" >{! option }</option>
		                            </aura:iteration>
		                        </lightning:select>                      
			                    </span> 
		                    
			                    <div class="slds-grid slds-wrap">
			                    <div class="slds-col slds-size_1-of-1 ">Company Corporate Address</div> 
			                    <br/>
			                    <br/>
			                    <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
			                    <lightning:input aura:id="newSSTform" label="Address Line 1" name="Address Line 1" value="{!v.sstData.Company_Corporate_Address_Address_Line__c}" 
			                     maxlength="35" required="true" />
			                    </span>
			                    <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
			                    <lightning:input aura:id="newSSTform" label="Address Line 2" name="Address Line 2" value="{!v.sstData.CompanyCorporate_Address_Address_Line2__c}"  
			                     maxlength="35"/>
			                    </span>
			                    <span class="slds-size_1-of-2 slds-max-small-size_1-of-2 slds-p-horizontal_medium slds-p-bottom_large"> 
			                    <lightning:input aura:id="newSSTform" label="City" name="City"  pattern="^[a-zA-Z]*$" messageWhenPatternMismatch="Please provide valid input." value="{!v.sstData.Company_Corporate_Address_City__c}" maxlength="22" required="true"/>
			                    </span>
			                     <span class="slds-size_1-of-2 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large" >
			                        <lightning:select aura:id="stateCopId" name="stateOfIn" label="State" value="{!v.sstData.Company_Corporate_Address_State__c}" required="true" >
			                            <aura:iteration var="option" items="{! v.stateOfInPicklist }">
			                                <option value="{! option }" >{! option }</option>
			                            </aura:iteration>
			                        </lightning:select>                      
			                    </span> 
			                    <span class="slds-size_1-of-2 slds-max-small-size_1-of-2 slds-p-horizontal_medium slds-p-bottom_large"> 
			                    <lightning:input aura:id="newSSTform" label="Zip" name="Zip" maxlength="9" value="{!v.sstData.Company_Corporate_Address_Zip_Code__c}"  required="true" />
			                    </span>
			                     <span class="slds-size_1-of-2 slds-max-small-size_1-of-2 slds-p-horizontal_medium slds-p-bottom_large"> 
			                    <lightning:input aura:id="newSSTform" label="Country" pattern="^[a-zA-Z]*$"  maxlength="2" messageWhenPatternMismatch="Please provide valid input." name="Country" value="{!v.sstData.Company_Corporate_Address_Country__c}"  required="true" />
			                     <lightning:helptext content="Please provide two letter country code." iconName="utility:info"/>
			                    </span>
			                     <span class="slds-size_1-of-2 slds-max-small-size_1-of-2 slds-p-horizontal_medium slds-p-bottom_large"> 
			                    <lightning:input aura:id="newSSTform" label="Phone Number" name="Phone Number" pattern="^[0-9]*$" messageWhenPatternMismatch="Please provide valid input." maxlength="10" value="{!v.sstData.Company_Corporate_Phone_Number__c}"  required="true" />
			                    </span>
			                     <span class="slds-size_1-of-2 slds-max-small-size_1-of-2 slds-p-horizontal_medium slds-p-bottom_large"> 
			                    <lightning:input aura:id="newSSTform" label="Phone Extension" name="Phone Extension" pattern="^[0-9]*$" messageWhenPatternMismatch="Please provide valid input." maxlength="5" value="{!v.sstData.Company_Corporate_Phone_Extension__c}"  />
			                    </span>
			                    <span class="slds-size_1-of-2 slds-max-small-size_1-of-2 slds-p-horizontal_medium slds-p-bottom_large">
			                    <lightning:input aura:id="sameAsMailing" type="checkbox" label="Same as mailing address?" name="sameAsMailing" checked="true" value="{!v.sstData.Same_as_mailing_address__c}"
			                             onchange="{! c.sameAsMailing}"/>
			                   </span>
			                   </div>
			                   
			                    <aura:if isTrue="{!v.sameAsMailing}">
			                    <div class="slds-grid slds-wrap">
			                    <div class="slds-col slds-size_1-of-1 ">Company Mailing Address</div> 
			                    <br/>
			                    <br/>
			                    <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
			                    <lightning:input aura:id="newSSTform" label="Address Line 1" name="Address Line 1" value="{!v.sstData.Company_Mailing_Address_Address_1__c}" 
			                     maxlength="35" required="true" />
			                    </span>
			                    <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
			                    <lightning:input aura:id="newSSTform" label="Address Line 2" name="Address Line 2" value="{!v.sstData.Company_Mailing_Address_Address_2__c}"  
			                     maxlength="35"/>
			                    </span>
			                    <span class="slds-size_1-of-2 slds-max-small-size_1-of-2 slds-p-horizontal_medium slds-p-bottom_large"> 
			                    <lightning:input aura:id="newSSTform" label="City" name="City" value="{!v.sstData.Company_Mailing_Address_City__c}" pattern="^[a-zA-Z]*$" messageWhenPatternMismatch="Please provide valid input." maxlength="22" required="true"/>
			                    </span>
			                     <span class="slds-size_1-of-2 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large" >
			                        <lightning:select aura:id="stateCopId" name="stateOfIn" label="State" value="{!v.sstData.Company_Mailing_Address_State__c}"  required="true" >
			                            <aura:iteration var="option" items="{! v.stateOfInPicklist }">
			                                <option value="{! option }" >{! option }</option>
			                            </aura:iteration>
			                        </lightning:select>                      
			                    </span> 
			                    <span class="slds-size_1-of-2 slds-max-small-size_1-of-2 slds-p-horizontal_medium slds-p-bottom_large"> 
			                    <lightning:input aura:id="newSSTform" label="Zip" name="Zip" maxlength="9" value="{!v.sstData.Company_Mailing_Address_Zip_Code__c}"  required="true" />
			                    </span>
			                     <span class="slds-size_1-of-2 slds-max-small-size_1-of-2 slds-p-horizontal_medium slds-p-bottom_large"> 
			                    <lightning:input aura:id="newSSTform" label="Country" pattern="^[a-zA-Z]*$"  maxlength="2" messageWhenPatternMismatch="Please provide valid input." name="Country" value="{!v.sstData.Company_Mailing_Address_Country__c}"  required="true" />
			                    <lightning:helptext content="Please provide two letter country code." iconName="utility:info"/>
			                    </span>
			                     <span class="slds-size_1-of-2 slds-max-small-size_1-of-2 slds-p-horizontal_medium slds-p-bottom_large"> 
			                    <lightning:input aura:id="newSSTform" label="Phone Number" pattern="^[0-9]*$" messageWhenPatternMismatch="Please provide valid input." maxlength="10" name="Phone Number" value="{!v.sstData.Company_Mailing_Phone_Extension__c}"  required="true" />
			                    </span>
			                     <span class="slds-size_1-of-2 slds-max-small-size_1-of-2 slds-p-horizontal_medium slds-p-bottom_large"> 
			                    <lightning:input aura:id="newSSTform" label="Phone Extension" pattern="^[0-9]*$" messageWhenPatternMismatch="Please provide valid input." maxlength="5" name="Phone Extension" value="{!v.sstData.Company_Corporate_Phone_Extension__c}"  />
			                    </span>
			                   </div>
			                   </aura:if>
			                   
			                   <!--Company Contact Information-->
			                	 <div class="slds-grid slds-wrap" >    
			                	 <div class="slds-col slds-size_1-of-1 "> Company Contact Information</div> 
			                  <br/>
			                  <br/>                    
			                     <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
			                     <lightning:input aura:id="newSSTform" label="First Name" name="First Name"  maxlength="16" 
			                     pattern="^[a-zA-Z0-9]*$" messageWhenPatternMismatch="Please provide valid input." value="{!v.sstData.Contact_First_Name__c}"  required="true"/>
			                     </span>
			                     <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
			                     <lightning:input aura:id="newSSTform" label="Middle Name" name="Middle Name" maxlength="16"
			                      pattern="^[a-zA-Z0-9]*$" messageWhenPatternMismatch="Please provide valid input."  value="{!v.sstData.Contact_Middle_Name__c}"  required="true"/>
			                     </span>
			                     <span class="slds-size_1-of-1 slds-max-small-size_1-of-1 slds-p-horizontal_medium slds-p-bottom_large"> 
			                     <lightning:input aura:id="newSSTform" label="Last Name" name="Last Name" maxlength="32"
			                      pattern="^[a-zA-Z0-9]*$" messageWhenPatternMismatch="Please provide valid input."  value="{!v.sstData.Contact_Last_Name__c}"  required="true" />
			                     </span>
