                    <button class="slds-button slds-button--brand" onclick="{!c.SSTStateTab}" >Next</button>
                </aura:renderIf>
                <aura:renderIf isTrue="{!v.showNexus}">        
                 <button class="slds-button slds-button--brand" onclick="{!c.prevTab}" >Back</button> 
                    <button class="slds-button slds-button--brand" onclick="{!c.NexusTab}" >Next</button>
                </aura:renderIf>
                 <aura:renderIf isTrue="{!v.showReg}">        
                 <button class="slds-button slds-button--brand" onclick="{!c.prevTab}" >Back</button> 
                    <button class="slds-button slds-button--brand" onclick="{!c.RegTab}" >Next</button>
                </aura:renderIf>
                <aura:renderIf isTrue="{!v.showStats}">        
                    <button class="slds-button slds-button--brand" onclick="{!c.prevTab}" >Back</button> 
                    <button class="slds-button slds-button--brand" onclick="{!c.statsTab}" >Next</button>
                </aura:renderIf>
                <aura:renderIf isTrue="{!v.showData}">        
                    <button class="slds-button slds-button--brand" onclick="{!c.prevTab}" >Back</button> 
                    <button class="slds-button slds-button--brand" onclick="{!c.saveRecord}" >Save</button>
                </aura:renderIf>
                
            </div>
    </div>
