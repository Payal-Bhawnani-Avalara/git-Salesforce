    <validationRules>
        <fullName>Invoiced</fullName>
        <active>true</active>
        <description>Invoice Number or Date is not allowed on record unless it is invoiced</description>
        <errorConditionFormula>(Invoice_Number__c != null ||
Invoice_Date__c != null) &amp;&amp; Invoiced__c = false</errorConditionFormula>
        <errorMessage>Invoice Number or Date is not allowed on record unless it is invoiced</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TimecardSplit_Assignment_Proj_Mismatch</fullName>
        <active>true</active>
        <description>Timecard Split Project must match Assignment Project.</description>
        <errorConditionFormula>AND(NOT(ISNULL(Assignment__c)),(Assignment__c &lt;&gt; &apos;&apos;),NOT(ISNULL(Assignment__r.Project__c)),(Assignment__r.Project__c &lt;&gt; &apos;&apos;),(Project__c &lt;&gt; Assignment__r.Project__c))</errorConditionFormula>
        <errorDisplayField>Assignment__c</errorDisplayField>
        <errorMessage>Timecard Split Project must match Assignment Project.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TimecardSplit_Assignment_Res_Mismatch</fullName>
        <active>true</active>
        <description>Timecard Split Resource must match Assignment Resource.</description>
        <errorConditionFormula>AND(NOT(ISNULL(Assignment__c)),(Assignment__c &lt;&gt; &apos;&apos;),NOT(ISNULL(Assignment__r.Resource__c)),(Assignment__r.Resource__c &lt;&gt; &apos;&apos;),(Resource__c &lt;&gt; Assignment__r.Resource__c))</errorConditionFormula>
        <errorDisplayField>Assignment__c</errorDisplayField>
        <errorMessage>Timecard Split Resource must match Assignment Resource.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TimecardSplit_Bill_Date_Locking</fullName>
        <active>true</active>
        <description>Bill Date is locked if Billed or Invoiced.</description>
        <errorConditionFormula>AND(OR(AND(Billed__c,NOT(ISCHANGED(Billed__c))),AND(Invoiced__c,NOT(ISCHANGED(Invoiced__c)))),ISCHANGED(Bill_Date__c))</errorConditionFormula>
        <errorDisplayField>Bill_Date__c</errorDisplayField>
        <errorMessage>Bill Date is locked if Billed or Invoiced.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TimecardSplit_Billing_Requires_Inclusion</fullName>
        <active>true</active>
        <description>For a Timecard Split to be marked as Billed, it must also be marked as Included in Financials and Approved.</description>
        <errorConditionFormula>AND(Billed__c,OR(NOT(Include_In_Financials__c),NOT(Approved__c)))</errorConditionFormula>
        <errorDisplayField>Billed__c</errorDisplayField>
        <errorMessage>For a Timecard Split to be marked as Billed, it must also be marked as Included in Financials and Approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TimecardSplit_Invoice_Date_Locking</fullName>
        <active>true</active>
        <description>Invoice Date is locked if Invoiced.</description>
        <errorConditionFormula>AND(Invoiced__c,NOT(ISCHANGED(Invoiced__c)),ISCHANGED(Invoice_Date__c))</errorConditionFormula>
        <errorDisplayField>Invoice_Date__c</errorDisplayField>
        <errorMessage>Invoice Date is locked if Invoiced.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TimecardSplit_Invoicing_Requires_Billing</fullName>
        <active>true</active>
        <description>For a Timecard Split to be marked as Invoiced, it must also be marked as Billed.</description>
        <errorConditionFormula>AND(Invoiced__c,NOT(Billed__c))</errorConditionFormula>
        <errorDisplayField>Invoiced__c</errorDisplayField>
        <errorMessage>For a Timecard Split to be marked as Invoiced, it must also be marked as Billed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TimecardSplit_Milestone_Project_Mismatch</fullName>
        <active>true</active>
        <description>Timecard Split Project must match Milestone Project.</description>
        <errorConditionFormula>AND(NOT(ISNULL(Milestone__c)),(Milestone__c &lt;&gt; &apos;&apos;),NOT(ISNULL(Milestone__r.Project__c)),(Milestone__r.Project__c &lt;&gt; &apos;&apos;),(Project__c &lt;&gt; Milestone__r.Project__c))</errorConditionFormula>
        <errorDisplayField>Milestone__c</errorDisplayField>
        <errorMessage>Timecard Split Project must match Milestone Project.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Timecard_Split_Project_is_Constant</fullName>
        <active>true</active>
        <description>A Timecard Split&apos;s Project field value may not be blank and may not be updated once set (unless Admin Global Edit is checked and Audit Notes are provided on the parent Timecard Header and Actuals Calculation Mode is Scheduled).</description>
        <errorConditionFormula>OR(ISNULL(Project__c),(Project__c=&apos;&apos;),AND(ISCHANGED(Project__c),OR(NOT(Timecard_Header__r.Admin_Global_Edit__c),ISNULL(Timecard_Header__r.Audit_Notes__c),ISBLANK(TRIM(Timecard_Header__r.Audit_Notes__c)))))</errorConditionFormula>
        <errorDisplayField>Project__c</errorDisplayField>
        <errorMessage>A Timecard Split&apos;s Project field value may not be blank and may not be updated once set (unless Admin Global Edit is checked and Audit Notes are provided on the parent Timecard Header and Actuals Calculation Mode is Scheduled).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Timecard_Split_Requires_Assignment</fullName>
        <active>true</active>
        <description>Timecard Splits for this Project and/or Resource require an Assignment.  Only if the Project OR the Resource allows Timecards without Assignment can the Assignment be blank on the Timecard Split.</description>
        <errorConditionFormula>AND(NOT(Project__r.Allow_Timecards_Without_Assignment__c),NOT(Resource__r.Allow_Timecards_Without_Assignment__c),OR(ISNULL(Assignment__c),(Assignment__c = &apos;&apos;)))</errorConditionFormula>
        <errorMessage>Timecard Splits for this Project and/or Resource require an Assignment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Timecard_Split_Resource_is_Constant</fullName>
        <active>true</active>
        <description>A Timecard Split&apos;s Resource field value may not be blank and may not be updated once set (unless Admin Global Edit is checked and Audit Notes are provided on the parent Timecard Header and Actuals Calculation Mode is Scheduled).</description>
        <errorConditionFormula>OR(ISNULL(Resource__c),(Resource__c=&apos;&apos;),AND(ISCHANGED(Resource__c),OR(NOT(Timecard_Header__r.Admin_Global_Edit__c),ISNULL(Timecard_Header__r.Audit_Notes__c),ISBLANK(TRIM(Timecard_Header__r.Audit_Notes__c)))))</errorConditionFormula>
        <errorDisplayField>Resource__c</errorDisplayField>
        <errorMessage>A Timecard Split&apos;s Resource field value may not be blank and may not be updated once set (unless Admin Global Edit is checked and Audit Notes are provided on the parent Timecard Header and Actuals Calculation Mode is Scheduled).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Vendor_Invoice_Constant</fullName>
        <active>true</active>
        <description>The vendor invoice item cannot be changed once set.</description>
        <errorConditionFormula>AND(ISCHANGED(Vendor_Invoice_Item__c),LEN(PRIORVALUE(Vendor_Invoice_Item__c))&gt;0)</errorConditionFormula>
        <errorDisplayField>Vendor_Invoice_Item__c</errorDisplayField>
        <errorMessage>The vendor invoice item cannot be changed once set.</errorMessage>
    </validationRules>
