<apex:page standardController="Contact" action="{!initForResource}" extensions="pse.UtilizationCalculatorController"
    title="{!$Label.pse__utilization_label_utilization_calc}" showHeader="false" sidebar="false" lightningStylesheets="true">

	<apex:outputPanel rendered="{!$User.UIThemeDisplayed == 'Theme4d'}" layout="none">
		<style>
			.pageTitleIcon {
				border-radius: .25rem;
			    background-color: #A094ED;
			    background-image: url("{!URLFOR($Asset.SLDS, 'assets/icons/standard/contact.svg')}");
			    background-repeat: no-repeat;
			    background-position: center;
			    background-size: 2rem;
				display: block !important;
			}
		</style>
	</apex:outputPanel>
    <apex:composition template="pse__UtilizationCalculator">
        <apex:define name="sectionHeader">
            <apex:sectionHeader title="{!$Label.pse__utilization_label_utilization_calc}" subtitle="{!Contact.Name}"/>
        </apex:define>
    </apex:composition>

</apex:page>