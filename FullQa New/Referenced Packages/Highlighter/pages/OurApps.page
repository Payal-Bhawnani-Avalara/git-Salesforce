            <script>
                var j$ = jQuery.noConflict();
                j$(document).ready(function(){
                    
                    j$('#highlighterImageText').hide();
                    j$('#universalPicklistImageText').hide();
                    j$('#listImageText').hide();
                    j$('#avText').hide();
                    j$('#imText').hide();
                    j$('#rmText').hide();
                    j$('#ssText').hide();
                    j$('#smText').hide();
                    j$('#usText').hide();
                    j$('#etaText').hide();
                    <!-- j$('#avfText').hide(); -->
                });
                function showMouseOver(imageId,divId){
                    showAll();
                    j$('#'+imageId).hide();
                    j$('#'+divId).show();
                }
                function hideMouseOver(imageId,divId){
                    showAll();
                    j$('#'+imageId).show();
                    j$('#'+divId).hide();
                }
                function showAll(){
                    j$('#highlighterImageText').hide();
                    j$('#universalPicklistImageText').hide();
                    j$('#listImageText').hide();
                    j$('#avText').hide();
                    j$('#ssText').hide();
                    j$('#smText').hide();
                    j$('#usText').hide();
                    j$('#etaText').hide();
                    <!--j$('#avfText').hide(); -->
                    j$('#imText').hide();
                    j$('#rmText').hide();
                    j$('#highlighterImage').show();
                    j$('#universalPicklistImage').show();
                    j$('#listImage').show();
                    j$('#av').show();
                    j$('#ss').show();
                    j$('#sm').show();
                    j$('#us').show();
                    j$('#eta').show();
                    <!--j$('#avf').show();    -->
                    j$('#im').show();
                    j$('#rm').show();    
                }
            </script>
