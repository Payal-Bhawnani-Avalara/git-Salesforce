    <validationRules>
        <fullName>Budget_Header_Account_Mismatch</fullName>
        <active>true</active>
        <errorConditionFormula>AND(LEN(Budget_Header__c)&gt;0, Budget_Header__r.Account__c != Account__c)</errorConditionFormula>
        <errorDisplayField>Budget_Header__c</errorDisplayField>
        <errorMessage>The budget header&apos;s account must match the account on this budget</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Budget_Header_Project_Mismatch</fullName>
        <active>true</active>
        <errorConditionFormula>AND(LEN(Budget_Header__c)&gt;0,Budget_Header__r.Project__c != Project__c)</errorConditionFormula>
        <errorDisplayField>Budget_Header__c</errorDisplayField>
        <errorMessage>The budget header&apos;s project does not match the project on this budget</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Budget_Header_Type_Mismatch</fullName>
        <active>false</active>
        <errorConditionFormula>AND(LEN(Budget_Header__c)&gt;0,TEXT(Budget_Header__r.Type__c) != TEXT(Type__c))</errorConditionFormula>
        <errorDisplayField>Type__c</errorDisplayField>
        <errorMessage>The budget header&apos;s type must match the type on this budget</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Budget_Project_is_Constant</fullName>
        <active>true</active>
        <description>A Budget&apos;s Project field value may not be blank and may not be updated once set (unless Admin Global Edit is checked, Audit Notes are provided, and Actuals Calculation Mode is Scheduled).</description>
        <errorConditionFormula>OR(ISNULL(Project__c),(Project__c=&apos;&apos;),AND(ISCHANGED(Project__c),OR(NOT(Admin_Global_Edit__c),ISNULL(Audit_Notes__c),ISBLANK(TRIM(Audit_Notes__c)))))</errorConditionFormula>
        <errorDisplayField>Project__c</errorDisplayField>
        <errorMessage>A Budget&apos;s Project field value may not be blank and may not be updated once set (unless Admin Global Edit is checked, Audit Notes are provided, and Actuals Calculation Mode is Scheduled).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Has_non_negative_budget_amount</fullName>
        <active>true</active>
        <description>This check will ensure that the budget amount is not a negative value</description>
        <errorConditionFormula>Amount__c   &lt;  0</errorConditionFormula>
        <errorDisplayField>Amount__c</errorDisplayField>
        <errorMessage>Amount should be a positive value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Has_non_negative_pre_billed_amount</fullName>
        <active>true</active>
        <description>This check will ensure that the budget pre billed amount is not a negative value</description>
        <errorConditionFormula>PreBilledAmount__c  &lt;  0</errorConditionFormula>
        <errorDisplayField>PreBilledAmount__c</errorDisplayField>
        <errorMessage>Pre Billed Amount should be a positive value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Master_Budget_is_Constant</fullName>
        <active>true</active>
        <description>Master Budget cannot be changed once set.</description>
        <errorConditionFormula>AND(ISCHANGED(Budget_Header__c),LEN(PRIORVALUE(Budget_Header__c))&gt;0)</errorConditionFormula>
        <errorDisplayField>Budget_Header__c</errorDisplayField>
        <errorMessage>Master Budget cannot be changed once set.</errorMessage>
    </validationRules>
