    <validationRules>
        <fullName>Clear_closed_date_with_open_status</fullName>
        <active>true</active>
        <description>The Closed Date must be blank if the status is set to Open.</description>
        <errorConditionFormula>AND( ISPICKVAL( Status__c , &apos;Open&apos;), NOT(ISBLANK( Closed_Date__c )))</errorConditionFormula>
        <errorMessage>Closed date must be blank if status is set to Open</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_date_greater_than_created_date</fullName>
        <active>true</active>
        <description>The risk&apos;s Closed Date must be after it was raised.</description>
        <errorConditionFormula>AND( ISPICKVAL( Status__c, &apos;Closed&apos;),Date_Raised__c &gt; Closed_Date__c)</errorConditionFormula>
        <errorMessage>Closed date for the risk must be after the date it was raised.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_date_mandatory_with_closed_status</fullName>
        <active>true</active>
        <description>The Closed Date cannot be blank if the status is set to Closed.</description>
        <errorConditionFormula>AND (ISPICKVAL( Status__c , &apos;Closed&apos;), ISBLANK( Closed_Date__c ))</errorConditionFormula>
        <errorMessage>Closed date can not be blank if status set to closed.</errorMessage>
    </validationRules>
