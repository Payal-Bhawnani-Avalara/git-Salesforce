    <validationRules>
        <fullName>Perm_Ctrl_CasscadeCheck_Not_Req</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(Team__c)), Cascading_Permission__c )</errorConditionFormula>
        <errorMessage>Should not have Casscading Permission checked when assigning Team</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Permission_Control_User_Required</fullName>
        <active>true</active>
        <description>Permission Controls must specify a User.</description>
        <errorConditionFormula>OR(ISNULL(User__c),(User__c=&apos;&apos;))</errorConditionFormula>
        <errorDisplayField>User__c</errorDisplayField>
        <errorMessage>Permission Controls must specify a User.</errorMessage>
    </validationRules>
