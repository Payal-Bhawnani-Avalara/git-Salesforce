    <validationRules>
        <fullName>Cannot_become_baseline_when_deleting</fullName>
        <active>true</active>
        <description>Error message indicating the version cannot be defined as a baseline as it is being deleted.</description>
        <errorConditionFormula>AND( Baseline__c  = true, ISPICKVAL (Status__c, &apos;Deleting&apos;))</errorConditionFormula>
        <errorMessage>Your selected version is queued for deletion and cannot be set as a baseline.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Version_should_have_a_name</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Name)</errorConditionFormula>
        <errorMessage>You must provide a name for this project version.</errorMessage>
    </validationRules>
