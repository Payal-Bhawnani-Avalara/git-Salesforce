    <validationRules>
        <fullName>Check_End_Date</fullName>
        <active>true</active>
        <errorConditionFormula>End_Date__c &lt;  Start_Date__c</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>End date cannot occur before start date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Hours</fullName>
        <active>true</active>
        <errorConditionFormula>OR(Sunday_Hours__c &lt; 0, Sunday_Hours__c &gt; 24, Monday_Hours__c &lt; 0, Monday_Hours__c &gt; 24, Tuesday_Hours__c &lt; 0, Tuesday_Hours__c &gt; 24, Wednesday_Hours__c &lt; 0, Wednesday_Hours__c &gt; 24, Thursday_Hours__c &lt; 0, Thursday_Hours__c &gt; 24, Friday_Hours__c &lt; 0, Friday_Hours__c &gt; 24, Saturday_Hours__c &lt; 0, Saturday_Hours__c &gt; 24)</errorConditionFormula>
        <errorMessage>Hours cannot be negative or greater than 24.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Time_must_be_before_End_Time</fullName>
        <active>true</active>
        <description>Validation message indicating the Schedule Start Time must be earlier than the End Time.</description>
        <errorConditionFormula>IF( AND(NOT(ISNULL(Monday_Start_Time__c)),Monday_Start_Time__c  &gt;  Monday_End_Time__c) , TRUE, IF(AND(NOT(ISNULL(Tuesday_Start_Time__c)),Tuesday_Start_Time__c  &gt;  Tuesday_End_Time__c),TRUE,IF(AND(NOT(ISNULL(Wednesday_Start_Time__c)),Wednesday_Start_Time__c  &gt;  Wednesday_End_Time__c),TRUE,IF(AND(NOT(ISNULL(Thursday_Start_Time__c)),Thursday_Start_Time__c  &gt;  Thursday_End_Time__c),TRUE,IF(AND(NOT(ISNULL(Friday_Start_Time__c)),Friday_Start_Time__c  &gt;  Friday_End_Time__c),TRUE,IF(AND(NOT(ISNULL(Saturday_Start_Time__c)),Saturday_Start_Time__c  &gt;  Saturday_End_Time__c),TRUE,IF(AND(NOT(ISNULL(Sunday_Start_Time__c)),Sunday_Start_Time__c  &gt;  Sunday_End_Time__c),TRUE,FALSE)))))))</errorConditionFormula>
        <errorMessage>The Start Time must be earlier than the End Time.</errorMessage>
    </validationRules>
