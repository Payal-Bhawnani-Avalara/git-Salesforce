    <fields>
        <fullName>AllocationStatus__c</fullName>
        <deprecated>false</deprecated>
        <description>Shows if the Revenue for this contract has been fully allocated, or needs reallocating.</description>
        <externalId>false</externalId>
        <formula>IF(
    RevenueAllocated__c,
    IMAGE(&apos;/img/msg_icons/confirm16.png&apos;, $Label.ffrr__revenueallocatedsuccessfully ) &amp; &apos; &apos; &amp; $Label.ffrr__revenueallocatedsuccessfully,
    IMAGE(&apos;/img/msg_icons/error16.png&apos;, $Label.ffrr__revenuenotallocated ) &amp;
    IF (
        Revenue__c = 0,
        &apos; &apos; &amp; $Label.ffrr__norevenuetoallocate,
        IF (
            NullSSPCount__c &gt; 0,
            &apos; &apos; &amp; SUBSTITUTE($Label.ffrr__nullsspcountgreaterthanzero, &apos;{0}&apos;, TEXT( NullSSPCount__c ) ),
            IF (
                TotalSSP__c = 0,
                &apos; &apos; &amp; $Label.ffrr__totalsspiszero,
                IF (
                    TotalNullAllocationRatioCount__c &gt; 0,
                    &apos; &apos; &amp; $Label.ffrr__revenueallocationnotrun,
                    &apos; &apos; &amp; $Label.ffrr__revenuenotallocated
                )
            )
        )
    )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Shows if the Revenue for this contract has been fully allocated, or needs reallocating.</inlineHelpText>
        <label>Allocation Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
