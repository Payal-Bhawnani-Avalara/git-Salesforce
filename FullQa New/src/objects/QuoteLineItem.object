IF(AND( Product2.TensoftRule__c =&quot;Roy&quot;,NOT(CONTAINS(Product2.Name,&quot;Sandbox&quot;)), Product2.GPSKU__c &lt;&gt;&quot;TS-IMPSRVCS&quot;,Product2.GPSKU__c &lt;&gt;&quot;AC-PS&quot;,Product2.GPSKU__c &lt;&gt; &quot;AC-MSMAILING&quot;, Product2.GPSKU__c &lt;&gt; &quot;AC-MSVAL&quot;, NOT(CONTAINS(Product2.Name,&quot;Processing Fee&quot;)),NOT(CONTAINS(Product2.Name,&quot;Integration Services&quot;)), Product2.GPSKU__c &lt;&gt; &quot;AC-MSPHONE&quot;,RoyaltyDiscountValue__c =-999), PricebookEntry.UnitPrice,Quantity *( UnitPrice  - ( UnitPrice  *  Discount))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
