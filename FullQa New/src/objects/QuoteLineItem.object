    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>Billing_Frequency_Required</fullName>
        <active>true</active>
        <description>Forces the user to navigate back to the Opportunity and populate the Billing Frequency field prior to adding Quote Line Items.</description>
        <errorConditionFormula>OR(
AND(CONTAINS($UserRole.Name,&quot;CAM&quot;), Product2.GPSKU__c&lt;&gt;&quot;AT-SANDBOX-EXT&quot;,  TEXT(Quote.Opportunity.BillingFrequency__c)=&quot;&quot;),

AND(NOT(CONTAINS($UserRole.Name,&quot;CAM&quot;)),TEXT(Quote.Opportunity.BillingFrequency__c)=&quot;&quot;))</errorConditionFormula>
        <errorMessage>Please navigate back to your Opportunity and populate the Billing Frequency drop-down before adding Quote Line Items.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Custom_Pricing_limited_on_Royalty_Items</fullName>
        <active>true</active>
        <description>Only certain Royalty Items qualify for custom pricing.  Attempts to enter custom pricing on non-qualifying royalty items will trip this rule.</description>
        <errorConditionFormula>AND( 
Product2.GPSKU__c &lt;&gt; &quot;AC-CERTCAPTURE-CERT&quot;,
Product2.GPSKU__c &lt;&gt; &quot;AF-MRS-DATATRAX-RTNS&quot;,
Product2.GPSKU__c &lt;&gt; &quot;AF-MRS-EPICOR-RTNS&quot;,
Product2.GPSKU__c &lt;&gt; &quot;AF-MRSOEM-RTNS&quot;,
Product2.GPSKU__c &lt;&gt; &quot;AF-MRSPROCFEE-RTNS&quot;,
Product2.GPSKU__c &lt;&gt; &quot;AF-MRS-RTNS&quot;,
Product2.GPSKU__c &lt;&gt; &quot;TS-RETURNS&quot;,
Product2.GPSKU__c &lt;&gt; &quot;VC-AVACERT-CERT&quot;,
Product2.GPSKU__c &lt;&gt; &quot;VC-AVACERTSAGE-CERT&quot;,
Product2.GPSKU__c &lt;&gt; &quot;VC-INFORV-CERT&quot;,
Product2.GPSKU__c &lt;&gt; &quot;AC-AVACERT-CERT&quot;,
Product2.GPSKU__c &lt;&gt; &quot;AC-AVACERTSAGE-CERT&quot;,
Product2.GPSKU__c &lt;&gt; &quot;AC-INFORV-CERT&quot;,
Product2.GPSKU__c &lt;&gt; &quot;ATB-QB-TXN&quot;,
Product2.GPSKU__c &lt;&gt; &quot;ATP-CALCDASH-TXN&quot;,
Product2.TensoftRule__c=&quot;Roy&quot;,
UnitPrice &lt;&gt;  PricebookEntry.UnitPrice,
NOT(ISPICKVAL(Product2.PriceLevel__c,&quot;PLUST&quot;)),
 PricebookEntry.UnitPrice &lt;&gt; 0)</errorConditionFormula>
        <errorDisplayField>UnitPrice</errorDisplayField>
        <errorMessage>Custom Pricing is not allowed on this item.  The Sales Price must be equal to the List Price.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Discount_Limit_on_1099_SKUs</fullName>
        <active>true</active>
        <description>1099 SKU&apos;s are limited to a 15% maximum Discount per our Partner Agreement.  This rule applies to Discounts applied through the standard Discount field as well as Discount&apos;s derived by manual alteration of the Sales Price on 1099 Line Items.</description>
        <errorConditionFormula>OR(AND(CONTAINS(Product2.Name,&quot;1099&quot;), Discount &gt; 0.15),AND(CONTAINS(Product2.Name,&quot;1099&quot;),( PricebookEntry.UnitPrice -  UnitPrice )/PricebookEntry.UnitPrice &gt; 0.15))</errorConditionFormula>
        <errorMessage>1099 Line Items cannot be Discounted more than 15% per the terms of our Partner Agreement with Convey.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>List_Price_Overrides_Sales_Price</fullName>
        <active>false</active>
        <description>Throws an error is a rep (non-CAM) attempts to alter the Sales Price of a Quote Line Item other than Royalty or Sandbox Items.</description>
        <errorConditionFormula>AND(  Quote.CustomPricing__c =false,
Product2.TensoftRule__c &lt;&gt;&quot;Roy&quot;, 
NOT(CONTAINS( Product2.Name ,&quot;Sandbox&quot;)), 
NOT(CONTAINS( Product2.Name ,&quot;Integration Services&quot;)),
NOT(ISPICKVAL(  Product2.PriceLevel__c ,&quot;PLUST&quot;)),
UnitPrice &lt;&gt; PricebookEntry.UnitPrice, 
NOT(CONTAINS($UserRole.Name,&quot;CAM&quot;)),
NOT(CONTAINS($UserRole.Name,&quot;Ava2 Business Development Manager&quot;)), 
AND(NOT(CONTAINS( Product2.Name ,&quot;MatrixMaster&quot;)),PricebookEntry.UnitPrice&lt;&gt;0)
)</errorConditionFormula>
        <errorDisplayField>UnitPrice</errorDisplayField>
        <errorMessage>You may only alter the Sales Price field on Usage Based Line Items (e.g. Transactions, Returns, Certs) and Sandboxes.  If you wish to discount any other Line Item you must do so by entering a Discount % in the &apos;Discount&apos; field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sales_Price_Must_Be_Entered_Manually</fullName>
        <active>true</active>
        <description>Products that require manual price calculations (like support plans) are entered with a default list price of (1).  This ensures that the person entering the item enters the correct calculated price.</description>
        <errorConditionFormula>AND(PricebookEntry.UnitPrice = -1,OR(UnitPrice = 0, UnitPrice = NULL))</errorConditionFormula>
        <errorDisplayField>UnitPrice</errorDisplayField>
        <errorMessage>This item requires manual entry of the Sale Price.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sandbox_Ext_requires_Term</fullName>
        <active>true</active>
        <description>Cannot add Sandbox Ext as a Quote Line item unless the Sandbox Term field is already populated.</description>
        <errorConditionFormula>AND(Product2.GPSKU__c =&quot;AT-SANDBOX-EXT&quot;,TEXT( Quote.SandBoxTerm_Months__c)=&quot;&quot;)</errorConditionFormula>
        <errorMessage>You must select a &apos;Sandbox Term&apos; on your Quote record before adding a Sandbox extension as a Quote Line Item.</errorMessage>
    </validationRules>
</CustomObject>
