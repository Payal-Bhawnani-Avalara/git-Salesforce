    <validationRules>
        <fullName>CurrentReconciliationRequiresExplanation</fullName>
        <active>true</active>
        <description>The user must fill in an Explanation if the reconciliation is for the current filing cycle</description>
        <errorConditionFormula>AND(ISPICKVAL( Type__c ,&quot;Current&quot;),ISBLANK( Explanation__c ))</errorConditionFormula>
        <errorDisplayField>Explanation__c</errorDisplayField>
        <errorMessage>You must fill in an Explanation if the reconciliation is for the current filing cycle!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Current_Reconciliation_Requires_At_Fault</fullName>
        <active>true</active>
        <description>The user must select a value in the At Fault field if the reconciliation is for the current filing cycle</description>
        <errorConditionFormula>AND( ISPICKVAL(Type__c,&quot;Current&quot;), ISPICKVAL(AtFault__c,&quot;&quot;))</errorConditionFormula>
        <errorDisplayField>AtFault__c</errorDisplayField>
        <errorMessage>You must select a value in the &apos;At Fault&apos; field because the reconciliation is for the current filing cycle!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fee_Required_to_complete_Current_Rec</fullName>
        <active>true</active>
        <description>The user must select a value in the &apos;Fee&apos; field before the Reconciliation can be completed if the reconciliation is for the current filing cycle</description>
        <errorConditionFormula>AND( ISPICKVAL(Type__c ,&quot;Current&quot;),ISPICKVAL( Fee__c ,&quot;&quot;),Recon_Complete__c =TRUE)</errorConditionFormula>
        <errorDisplayField>Fee__c</errorDisplayField>
        <errorMessage>You must select a value in the &apos;Fee&apos; field before the Reconciliation can be completed if the reconciliation is for the current filing cycle</errorMessage>
    </validationRules>
