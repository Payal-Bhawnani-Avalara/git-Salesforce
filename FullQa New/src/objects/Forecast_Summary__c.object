    <fields>
        <fullName>User_Quota__c</fullName>
        <description>User by Sales Operations for dynamic quota updates on forecast summary from user record.</description>
        <externalId>false</externalId>
        <formula>IF( 
MONTH( Forecast_Date__c ) = 1, 
Owner:User.Booking_Quota_January__c, 
IF( 
MONTH( Forecast_Date__c ) = 2, 
Owner:User.Booking_Quota_February__c, 
IF( 
MONTH( Forecast_Date__c ) = 3, 
Owner:User.Booking_Quota_March__c, 
IF( 
MONTH( Forecast_Date__c ) = 4, 
Owner:User.Booking_Quota_April__c, 
IF( 
MONTH( Forecast_Date__c ) = 5, 
Owner:User.Booking_Quota_May__c, 
IF( 
MONTH( Forecast_Date__c ) = 6, 
Owner:User.Booking_Quota_June__c, 
IF( 
MONTH( Forecast_Date__c ) = 7, 
Owner:User.Booking_Quota_July__c, 
IF( 
MONTH( Forecast_Date__c ) = 8, 
Owner:User.Booking_Quota_August__c, 
IF( 
MONTH( Forecast_Date__c ) = 9, 
Owner:User.Booking_Quota_September__c, 
IF( 
MONTH( Forecast_Date__c ) = 10, 
Owner:User.Booking_Quota_October__c, 
IF( 
MONTH( Forecast_Date__c ) = 11, 
Owner:User.Booking_Quota_November__c, 
IF( 
MONTH( Forecast_Date__c ) = 12, 
Owner:User.Booking_Quota_December__c, 
0))))))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>User Quota</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
