    <label>Forecast Summary</label>
    <listViews>
        <fullName>My_Forecast_Summaries_This_Month</fullName>
        <columns>NAME</columns>
        <filterScope>Mine</filterScope>
        <filters>
            <field>Forecast_Date__c</field>
            <operation>greaterThan</operation>
            <value>LAST_MONTH</value>
        </filters>
        <filters>
            <field>Forecast_Date__c</field>
            <operation>lessThan</operation>
            <value>NEXT_MONTH</value>
        </filters>
        <label>My Forecast Summaries This Month</label>
    </listViews>
