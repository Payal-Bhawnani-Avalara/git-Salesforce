    <validationRules>
        <fullName>All_BMW_Fields_must_be_entered</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Best_Case_Amount__c &lt; 1, Most_Likely_Amount__c &lt; 1,  $Profile.Name = &quot;Ava2 CAM&quot;)</errorConditionFormula>
        <errorMessage>Best case and most likely must be greater than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Locked_Forecast</fullName>
        <active>false</active>
        <errorConditionFormula>AND($Profile.Name = &quot;Ava2 CAM&quot;,  Locked__c = TRUE)</errorConditionFormula>
        <errorMessage>This forecast summary is currently locked. It will open 3 days before the forecast date and remain open for 2 days after the forecast date.</errorMessage>
    </validationRules>
