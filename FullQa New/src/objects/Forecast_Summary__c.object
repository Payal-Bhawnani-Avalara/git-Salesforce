            <operation>equals</operation>
            <value>yes</value>
        </filters>
        <filters>
            <field>Forecast_Date__c</field>
            <operation>greaterThan</operation>
            <value>TODAY</value>
        </filters>
        <label>My Team Forecasts Next month</label>
        <sharedTo>
