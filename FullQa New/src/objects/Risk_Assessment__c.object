var records = getRASS.getArray(&quot;records&quot;); 

{if (getRASS.size&gt;0) {
     var RASS_Id = records[0].Id;
     var RASS_name  = records[0].Name;

  // pre-fill the form by copying data from the most recent risk assessment 

custObjs[0].Risk__c = records[0].Risk__c; 
custObjs[0].Issue_Product__c = records[0].Issue_Product__c; 
