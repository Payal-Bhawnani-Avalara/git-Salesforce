        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>New Assessment (GoLive)</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/13.0/connection.js&quot;)} 
//**  This routine creates a new Risk Assessment object
//**  Then it queries the SalesForce database to retrieve the contents of the LAST 
//**  Risk Assessment for this Contract &amp; GoLive and populates the content of the NEW
//**  Risk Assessment.  (in essence, similar to cloning
//**  It then sets a few flags based on conditions:
//**  - if first Risk Assessment for a GoLive, flags as such
//**  - flags the new Risk Assessment as &quot;Most Recent&quot; and clears that flag for the 
//**    previous Risk Assessment.

//**  This routine is used to get the left x characters
function Left(str, n){
	if (n &lt;= 0)
	    return &quot;&quot;;
	else if (n &gt; String(str).length)
	    return str;
	else
	    return String(str).substring(0,n);
}

var OriginalURL = parent.location.href;
var url_root=Left(OriginalURL, 27)
//var CurrentID = parent.location.id;
//alert(&apos;OriginalURL: &apos; + OriginalURL);

//**  Create a new Risk Assessment object.
var custObjs = new Array(new sforce.SObject(&quot;Risk_Assessment__c&quot;)); 

// set Contract ID and GoLive ID, so that this new Assessment is linked
var IsProdContractEmpty = {!ISNULL(GoLive__c.Link_to_Production_AccountId__c)}
//alert(&apos;IsProdContractEmpty (1): &apos; + IsProdContractEmpty);
{
	if (IsProdContractEmpty)
	   custObjs[0].Contract__c = &apos;{!GoLive__c.Link_to_Dev_ContractId__c}&apos;;
	else
	   custObjs[0].Contract__c = &apos;{!GoLive__c.Link_to_Production_AccountId__c}&apos;;
}

//alert(&apos;IsProdContractEmpty (2): &apos; + IsProdContractEmpty);
 
custObjs[0].GoLive__c = &apos;{!GoLive__c.Id}&apos;;

//**  Also fill in the Product Family, Connector, and set the Most Recent flag
custObjs[0].Product_Family__c = &apos;{!GoLive__c.Product_Family__c}&apos;;
custObjs[0].Connector__c = &apos;{!GoLive__c.Connector2__c}&apos;;
custObjs[0].Most_recent__c = &apos;True&apos;;

// search for most recent Assessment so that we can &quot;clone&quot; it
var getRASS = sforce.connection.query(&quot;SELECT Id, Name, Risk__c, Issue_Product__c, Resolution_Product__c, Risk_Customer__c, Issue_Customer__c, Resolution_Customer__c, Risk_Schedule__c, Issue_Schedule__c, Resolution_Schedule__c, Risk_Terms__c, Issue_Terms__c, Resolution_Terms__c, Risk_Payment__c, Issue_Payment__c, Resolution_Payment__c, Contract__c, GoLive__c, Most_recent__c FROM Risk_Assessment__c  WHERE Contract__c = &apos;&quot; + custObjs[0].Contract__c + &quot;&apos; AND GoLive__c = &apos;&quot; + custObjs[0].GoLive__c + &quot;&apos; ORDER BY CreatedDate DESC&quot; );

var records = getRASS.getArray(&quot;records&quot;); 
//alert(getRASS.size);

{if (getRASS.size&gt;0) {

     var RASS_Id = records[0].Id;
     var RASS_name  = records[0].Name;

     // pre-fill the form by copying data from the most recent risk assessment

     custObjs[0].Risk__c = records[0].Risk__c;
     custObjs[0].Issue_Product__c = records[0].Issue_Product__c;
     custObjs[0].Resolution_Product__c = records[0].Resolution_Product__c;

     custObjs[0].Risk_Customer__c = records[0].Risk_Customer__c;
     custObjs[0].Issue_Customer__c = records[0].Issue_Customer__c;
     custObjs[0].Resolution_Customer__c = records[0].Resolution_Customer__c;

     custObjs[0].Risk_Schedule__c = records[0].Risk_Schedule__c;
     custObjs[0].Issue_Schedule__c = records[0].Issue_Schedule__c;
     custObjs[0].Resolution_Schedule__c = records[0].Resolution_Schedule__c;

     custObjs[0].Risk_Terms__c = records[0].Risk_Terms__c;
     custObjs[0].Issue_Terms__c = records[0].Issue_Terms__c;
     custObjs[0].Resolution_Terms__c = records[0].Resolution_Terms__c;

     custObjs[0].Risk_Payment__c = records[0].Risk_Payment__c;
     custObjs[0].Issue_Payment__c = records[0].Issue_Payment__c;
     custObjs[0].Resolution_Payment__c = records[0].Resolution_Payment__c;

     custObjs[0].Initial_GoLive_Review__c = &apos;False&apos;;


// Used to clear the &quot;Most Recent&quot; flag on the previous Risk Assessment
var getRASS2 = sforce.connection.query(&quot;SELECT Id, Most_recent__c  FROM Risk_Assessment__c  WHERE Contract__c = &apos;&quot; + custObjs[0].Contract__c + &quot;&apos; AND GoLive__c = &apos;&quot; + custObjs[0].GoLive__c + &quot;&apos; ORDER BY CreatedDate DESC&quot; );

     var records2 = getRASS2.getArray(&quot;records&quot;);
     records2[0].Most_recent__c = &apos;false&apos;;
     result = sforce.connection.update(records2);

}

else
{
//**  for first Risk Assessment, set the Initial GoLive Review flag to True
custObjs[0].Initial_GoLive_Review__c = &apos;True&apos;;
custObjs[0].Most_recent__c = &apos;True&apos;;
}

// Used to set the &quot;Last Risk Assessment&quot; field on the GoLive to the current date
var getRASS3 = sforce.connection.query(&quot;SELECT Id, Last_Risk_Assessment__c  FROM GoLive__c  WHERE  ID = &apos;&quot; + custObjs[0].GoLive__c + &quot;&apos; ORDER BY CreatedDate DESC&quot; );
var currentTime = new Date()
var month = currentTime.getMonth() + 1
var day = currentTime.getDate()
var year = currentTime.getFullYear()

     var records3 = getRASS3.getArray(&quot;records&quot;);
     records3[0].Last_Risk_Assessment__c = month + &quot;/&quot; + day + &quot;/&quot; + year;
     result = sforce.connection.update(records3);
//alert(result);
}

// create the risk assessment
var result = sforce.connection.create(custObjs);
// alert(result);

// open the new risk assessment
var new_url = url_root + result[0].id + &apos;/e?retURL=%2F&apos; + result[0].id ;     //the slash e opens the page in edit mode
parent.location.href = new_url  //refresh the page
//window.open(new_url, &apos;_blank&apos; // &lt;- This is what makes it open in a new window.</url>
