<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <actionOverrides>
        <actionName>Delete</actionName>
        <type>Default</type>
    </actionOverrides>
    <actionOverrides>
        <actionName>EditOpportunitySplits</actionName>
        <type>Default</type>
    </actionOverrides>
    <actionOverrides>
        <actionName>SaveAndClose</actionName>
        <type>Default</type>
    </actionOverrides>
    <actionOverrides>
        <actionName>SaveEdit</actionName>
        <type>Default</type>
    </actionOverrides>
    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>ARR_Split_Value__c</fullName>
        <externalId>false</externalId>
        <formula>IF(Opportunity.PROS_ARR__c = 0,Opportunity.ARR__c *SplitPercentage,Opportunity.PROS_ARR__c  *  SplitPercentage)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Split ARR Value</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>OpportunityId</fullName>
        <type>MasterDetail</type>
    </fields>
    <fields>
        <fullName>Partner_Sales_Group__c</fullName>
        <description>Used to report split values of opportunities by team</description>
        <externalId>false</externalId>
        <formula>TEXT(SplitOwner.Sales_Group__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Partner Sales Group</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>SplitAmount</fullName>
    </fields>
    <fields>
        <fullName>SplitNote</fullName>
    </fields>
    <fields>
        <fullName>SplitOwnerId</fullName>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>SplitPercentage</fullName>
    </fields>
    <fields>
        <fullName>SplitTypeId</fullName>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Split_Booked_Value__c</fullName>
        <description>Adjusted Booked Value based on percentage of Split
Integrated: No
Sales Operations - Amy Mosher</description>
        <externalId>false</externalId>
        <formula>Opportunity.BookedValue__c  *  SplitPercentage</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Split Booked Value</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Team_Member_Manager__c</fullName>
        <externalId>false</externalId>
        <formula>SplitOwner.Manager.Full_Name__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Team Member Manager</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Team_Member_Role__c</fullName>
        <externalId>false</externalId>
        <formula>SplitOwner.UserRole.Name</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Team Member Role</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Team_Name__c</fullName>
        <description>Used for dashboard reporting to show team and manager name on in one column</description>
        <externalId>false</externalId>
        <formula>Text(SplitOwner.Sales_Group__c) &amp; &quot; - &quot; &amp;  SplitOwner.Manager.LastName</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Team Name</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>User_Manager__c</fullName>
        <description>https://jira.avalara.com/browse/IBST-8080</description>
        <externalId>false</externalId>
        <formula>SplitOwner.Manager.Full_Name__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>User Manager</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <sharingModel>ControlledByParent</sharingModel>
</CustomObject>
