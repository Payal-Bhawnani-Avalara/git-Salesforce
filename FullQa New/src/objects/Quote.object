    <fields>
        <fullName>DCLCongaFlag__c</fullName>
        <description>Sets the error message that is output if Conga has been disabled as a result of pre-proposal generation validation has not been met</description>
        <externalId>false</externalId>
        <formula>IF(
OR( HasBillingAddress__c=&quot;No&quot;, HasShippingAddress__c=&quot;No&quot;),&quot;Please+enter+a+Billing/Shipping+Address&quot;,

IF(ISBLANK(Opportunity.BillingContact__c),&quot;Please+enter+a+Billing+Contact&quot;,

IF(AND(HasSingleState__c&gt;0, TEXT(Opportunity.SingleState__c)=&quot;&quot;),&quot;Please+populate+Single+State+on+Opp&quot;,
 
IF(AND(HasSingleState__c=0, TEXT(Opportunity.SingleState__c)&lt;&gt;&quot;&quot;),&quot;Missing+Single+State+Line+Items&quot;,

IF(AND(ISPICKVAL(Opportunity.Type,&quot;New&quot;), OR(TEXT(Opportunity.BillingFrequency__c)=&quot;&quot;,
ISBLANK(Opportunity.InitialContractTerm__c))),&quot;Billing+Frequency/Initial+Term+Required+on+Opp&quot;,

IF(TEXT(Opportunity.BillingFrequency__c)=&quot;&quot;,&quot;Billing+Frequency+Required+on+Opportunity&quot;,

IF(OR(AND( Has_Gold_Support__c &gt; 0, FreeMonthsValue__c &gt; 0, SandboxTermValue__c &lt; FreeMonthsValue__c),
AND(Has_Gold_Support__c &gt; 0, FreeMonthsValue__c &lt; 1, SandboxTermValue__c &lt;&gt; 1)),&quot;Sandbox+Term+cannot+be+less+than+Free+Months&quot;,

IF(AND( CustomPricing__c = true,  CustomPricingApproved__c = false),&quot;Custom+pricing+approval+required&quot;,

&quot;&quot;))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Sets the error message that is output if Conga has been disabled as a result of pre-proposal generation validation has not been met</inlineHelpText>
