    <fields>
        <fullName>Churn_Risk_Rating__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Churn_Risk__c &lt;16,&quot;Low&quot;,IF(Churn_Risk__c &lt;46,&quot;Medium&quot;,IF(Churn_Risk__c &gt;45,&quot;High&quot;,IF(Churn_Risk__c=0,&quot;*&quot;,IF(ISNULL(Churn_Risk__c),&quot;*&quot;,&quot;&quot;) ))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Churn Risk Rating</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
