        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Churn_Risk__c</fullName>
        <externalId>false</externalId>
        <formula>IF( AND(Account_Name__r.Open_Cases__c=0),((5- Total_CHI__c )+(10- NPS_Score_Average__c )),((5- Total_CHI__c )+(10- NPS_Score_Average__c )+ (Account_Name__r.Open_Cases__c )+(( Account_Name__r.Escalated_Cases__c /Account_Name__r.Open_Cases__c)*10)+(( Account_Name__r.Average_Open_Case_Age__c /60)*10)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Churn Risk</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <trackHistory>false</trackHistory>
