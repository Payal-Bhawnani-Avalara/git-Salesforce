IF(CONTAINS( ProductName__c ,&quot;Enterprise Support&quot;),&quot;Enterprise Support&quot;,

IF(OR(CONTAINS(  ProductName__c ,&quot;CertCapture Managed&quot;),CONTAINS(  ProductName__c ,&quot;CertCapture Managed&quot;)),&quot;Avalara CertCapture MS&quot;,

IF(AND(OR(CONTAINS(ProductName__c,&quot;Global&quot;),CONTAINS(ProductName__c,&quot;GLOBAL&quot;)),NOT(CONTAINS(ProductName__c,&quot;GlobalEdge&quot;))),&quot;GLOBAL&quot;,

  ProductFamily__c ))))))))))))))</formula>
