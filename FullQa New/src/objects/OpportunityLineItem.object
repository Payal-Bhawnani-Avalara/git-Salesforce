        <externalId>false</externalId>
        <formula>IF(CONTAINS(  ProductName__c  ,&quot;VAT Expert&quot;),&quot;VAT Expert&quot;,
IF(CONTAINS(  ProductName__c  ,&quot;VAT Reporting - Country Module&quot;),&quot;VAT Reporting&quot;,

