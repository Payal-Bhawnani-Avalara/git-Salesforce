    <fields>
        <fullName>GLIQualified__c</fullName>
        <externalId>false</externalId>
        <formula>IF(OR( 

Name =&quot;Avalara Returns - SST&quot;,

OR(CONTAINS(Name,&quot;NEXUS&quot;),CONTAINS(Name,&quot;REGISTRATIONS&quot;),CONTAINS(Name,&quot;BACK&quot;)),

AND(  NOT(ISPICKVAL(ProductFamily__c,&quot;Avalara TaxRates&quot;)),
NOT(ISPICKVAL(ProductFamily__c,&quot;Avalara SalesTax II&quot;)),
NOT(CONTAINS(Name,&quot;QUICKSTART&quot;)),
NOT(CONTAINS(Name,&quot;DEVELOPMENT ACCOUNT&quot;)), 
NOT(ISPICKVAL(ProductFamily__c,&quot;Additional Fees&quot;)),
OR( IsNFR__c = FALSE, AND(IsNFR__c = TRUE,Name = &quot;Avalara AvaTax - CALC DASH&quot;)))),

TRUE,FALSE)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
