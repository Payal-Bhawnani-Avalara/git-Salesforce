    <description>IBST-11861 - New Flow</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
NOT(ISBLANK([Case].RecordTypeId)),
[Case].RecordType.DeveloperName &lt;&gt; &quot;MyLodge&quot;, 
[Case].RecordType.DeveloperName &lt;&gt; &quot;MRS_Registration_Country&quot;, 
OR(
ISNEW(),
ISCHANGED([Case].Implementation_GLI_Trigger__c),
ISCHANGED([Case].CX_911_Support_Product__c ),
ISCHANGED([Case].Connector__c ),

(AND(
ISCHANGED([Case].Data_Utility__c ) ,
[Case].Data_Utility__c =&quot;GLAU&quot;

)
)
)

)</stringValue>
            </value>
