        <fullName>Live_date_must_be_after_start_date</fullName>
        <active>true</active>
        <errorConditionFormula>OR(AND(Target_AvaTax_Live_Date__c &lt;=  Implementation_Start_Date__c, Target_CertCapture_Live_Date__c  &lt;=  Implementation_Start_Date__c,
Target_Returns_Live_Date__c  &lt;=  Implementation_Start_Date__c),
AND(ISNULL(Target_AvaTax_Live_Date__c),ISNULL(Target_CertCapture_Live_Date__c),ISNULL(Target_Returns_Live_Date__c)))</errorConditionFormula>
        <errorMessage>Please enter a target live date later than the Implementation Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_After_Today</fullName>
        <active>true</active>
        <errorConditionFormula>OR(Implementation_Start_Date__c &lt;= TODAY(),ISNULL(Implementation_Start_Date__c))</errorConditionFormula>
        <errorDisplayField>Implementation_Start_Date__c</errorDisplayField>
        <errorMessage>You must enter a date which is after today.</errorMessage>
    </validationRules>
