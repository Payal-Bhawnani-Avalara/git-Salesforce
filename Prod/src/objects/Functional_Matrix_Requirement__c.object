    <validationRules>
        <fullName>Migration_Date_Already_Exists</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
NOT(ISNULL(PRIORVALUE(Date__c))) , 
isChanged(Date__c) 
)</errorConditionFormula>
        <errorMessage>Migration Date Already Exists</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Migration_Note_Exists</fullName>
        <active>false</active>
        <description>Used only for Note migration</description>
        <errorConditionFormula>PRIORVALUE(Note__c) &lt;&gt; NULL &amp;&amp; ISCHANGED(Note__c)</errorConditionFormula>
        <errorDisplayField>Note__c</errorDisplayField>
        <errorMessage>Note already exists</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_new_records_allowed_during_Migration</fullName>
        <active>false</active>
        <description>Used only for Note migration</description>
        <errorConditionFormula>isNew() &amp;&amp;  Note_Migrated__c == true</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>Error : No new records allowed during Note / Date Migration</errorMessage>
    </validationRules>
