    <validationRules>
        <fullName>Cannot_Add_Hold_and_Credit_Amount_Same</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Transaction_Limit_Hold__c = TRUE,  Transaction_Credit_Amount__c &gt; 0)</errorConditionFormula>
        <errorMessage>You cannot simultaneously add a credit and a limit on the same record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Credit_Balance_Greater_Than_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>Transaction_Credit_Balance__c &gt;  Transaction_Credit_Amount__c</errorConditionFormula>
        <errorMessage>The transaction credit balance cannot be greater than the transaction credit amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_Select_Credit_Amount_or_Limit_Hold</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISBLANK(Transaction_Credit_Amount__c),
Transaction_Limit_Hold__c = FALSE
)</errorConditionFormula>
        <errorMessage>You must enter a transaction credit amount or limit hold.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Transaction_Amount_after_Approval</fullName>
        <active>true</active>
        <description>https://jira.avalara.com/browse/IBST-12706</description>
        <errorConditionFormula>AND(
ISPICKVAL(Approval_Status__c, &quot;Approved&quot;),
ISCHANGED( Transaction_Credit_Amount__c )
)</errorConditionFormula>
        <errorMessage>You can not change the transaction credit amount after the ZBUC has been approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Transaction_Credit_Amount_Must_Be_Positi</fullName>
        <active>true</active>
        <errorConditionFormula>Transaction_Credit_Amount__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Transaction_Credit_Amount__c</errorDisplayField>
        <errorMessage>Please update Transaction Credit Amount to positive value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Transaction_Credit_Amount_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
Transaction_Limit_Hold__c = FALSE,  NOT(ISBLANK(Transaction_Credit_Amount__c )),
Transaction_Credit_Amount__c &lt;= 0

)</errorConditionFormula>
        <errorMessage>You must enter a Transaction Credit Amount.</errorMessage>
    </validationRules>
