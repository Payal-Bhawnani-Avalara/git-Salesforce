    <fields>
        <fullName>Calc_SAM_Booked_Daily_Target__c</fullName>
        <externalId>false</externalId>
        <formula>IF(MONTH(Target_Date__c) &lt; MONTH(TODAY()),  Target_Bookings__c  , (Target_Bookings__c/30)*DAY(TODAY()))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Calc SAM Booked Daily Target</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
