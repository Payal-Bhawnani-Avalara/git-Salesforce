		      	isText = $.trim(isText);

		        var convert = isText.indexOf('&lt;')!==-1
		          || isText.indexOf('&gt;')!==-1 ?
		            isText.text().replace(/&lt;/g,'<').replace(/&gt;/g,'>')
		            : isText;

