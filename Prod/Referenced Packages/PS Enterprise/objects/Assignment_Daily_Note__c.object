        <formula>IF(OR(ISNULL(Assignment__c),(Assignment__c=&apos;&apos;),ISNULL(Assignment__r.Project__c),(Assignment__r.Project__c=&apos;&apos;)),null,HYPERLINK(&apos;/&apos;&amp;Assignment__r.Project__c,Assignment__r.Project__r.Name))</formula>
