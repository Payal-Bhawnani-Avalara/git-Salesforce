    <validationRules>
        <fullName>Close_date_mandatory_if_status_is_closed</fullName>
        <active>true</active>
        <description>The Closed Date cannot be blank if the status is set to Completed or Closed - No Action.</description>
        <errorConditionFormula>(AND(OR(ISPICKVAL(Status__c,&apos;Completed&apos;), ISPICKVAL(Status__c,&apos;Closed - No action&apos;)), ISBLANK(Closed_Date__c)))</errorConditionFormula>
        <errorMessage>The Closed Date cannot be blank if the status is set to Completed or Closed - No Action.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Date_Blank_If_Status_Not_Closed</fullName>
        <active>true</active>
        <description>The Closed date must be blank if the status is set to  Not Yet Started, In Progress or On Hold.</description>
        <errorConditionFormula>AND(OR( ISPICKVAL(Status__c ,&apos;Not Yet Started&apos;),ISPICKVAL(Status__c ,&apos;In Progress&apos;),ISPICKVAL(Status__c ,&apos;On Hold&apos;)), (!ISBLANK(Closed_Date__c ))
)</errorConditionFormula>
        <errorMessage>The Closed date must be blank if the status is set to  Not Yet Started, In Progress or On Hold.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Date_Greater_Than_Date_Raised</fullName>
        <active>true</active>
        <description>The issue&apos;s closed date must be after it was raised.</description>
        <errorConditionFormula>Closed_Date__c&lt; Date_Raised__c</errorConditionFormula>
        <errorMessage>The issue&apos;s closed date must be after it was raised.</errorMessage>
    </validationRules>
