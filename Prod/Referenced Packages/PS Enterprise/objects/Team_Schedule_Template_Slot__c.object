        <errorConditionFormula>OR(Hours__c &lt; 0 , MOD(Hours__c ,0.25)  &lt;&gt; 0)</errorConditionFormula>
        <errorDisplayField>Hours__c</errorDisplayField>
        <errorMessage>Hours should be non-negative values in increments of 0.25.</errorMessage>
    </validationRules>
