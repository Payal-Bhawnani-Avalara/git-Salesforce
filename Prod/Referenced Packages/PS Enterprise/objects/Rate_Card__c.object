        <errorConditionFormula>OR(AND(NOT(OR(ISNULL(Account__c),(Account__c=&apos;&apos;))),NOT(AND(OR(ISNULL(Region__c),(Region__c=&apos;&apos;)),OR(ISNULL(Practice__c),(Practice__c=&apos;&apos;)),OR(ISNULL(Group__c),(Group__c=&apos;&apos;))))),AND(NOT(OR(ISNULL(Region__c),(Region__c=&apos;&apos;))),NOT(AND(OR(ISNULL(Account__c),(Account__c=&apos;&apos;)),OR(ISNULL(Practice__c),(Practice__c=&apos;&apos;)),OR(ISNULL(Group__c),(Group__c=&apos;&apos;))))),AND(NOT(OR(ISNULL(Practice__c),(Practice__c=&apos;&apos;))),NOT(AND(OR(ISNULL(Region__c),(Region__c=&apos;&apos;)),OR(ISNULL(Account__c),(Account__c=&apos;&apos;)),OR(ISNULL(Group__c),(Group__c=&apos;&apos;))))),AND(NOT(OR(ISNULL(Group__c),(Group__c=&apos;&apos;))),NOT(AND(OR(ISNULL(Region__c),(Region__c=&apos;&apos;)),OR(ISNULL(Practice__c),(Practice__c=&apos;&apos;)),OR(ISNULL(Account__c),(Account__c=&apos;&apos;))))))</errorConditionFormula>
        <errorMessage>Rate Card must not reference more than one of the following: Account, Region, Practice, Group.</errorMessage>
    </validationRules>
    <validationRules>
