<aura:component implements="flexipage:availableForAllPageTypes" access="global" >
  <aura:attribute name="entityId" type="String" access="GLOBAL"/>
  <aura:attribute name="entityType" type="String" access="GLOBAL"/>
  <aura:attribute name="msg" type="String" access="GLOBAL"/>
  <aura:attribute name="errMsg" type="String" access="GLOBAL"/>
  <aura:attribute name="btnLabel" type="String" access="GLOBAL"/>
    
  <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
  <aura:registerEvent name="navigateToURL" type="CameleonCPQ:navigateToURL"/>

  <ui:message title="Error" severity="error" closable="true">
     <ui:outputText value="{!v.errMsg}" />
  </ui:message>
    
  <div class="modal-footer slds-modal__footer">
	<div class="forceModalActionContainer--footerAction forceModalActionContainer">
      <lightning:button label="{!v.btnLabel}" onclick="{!c.redirect}"/>
    </div>
  </div>
</aura:component>