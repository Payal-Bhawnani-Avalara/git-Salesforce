    <fields>
        <fullName>brating__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(bscore__c), NULL,
IF(bscore__c &gt;= $Setup.Infer_Scoring_Settings__c.Score_Bucket_1_Threshold__c, 1,
IF(bscore__c &gt;= $Setup.Infer_Scoring_Settings__c.Score_Bucket_2_Threshold__c, 2,
IF(bscore__c &gt;= $Setup.Infer_Scoring_Settings__c.Score_Bucket_3_Threshold__c, 3,
4))))</formula>
        <label>Infer: Behavior Score [1-4]</label>
        <precision>18</precision>
